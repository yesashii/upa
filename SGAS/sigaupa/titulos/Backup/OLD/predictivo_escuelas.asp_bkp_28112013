<script language="JavaScript">
function abrir_datos(codigo,plan)
{
    direccion = "predictivo_escuelas_detalle.asp?pers_ncorr="+codigo+"&carr_ccod="+'<%=carr_ccod%>'+"&plan_ccod="+plan;
    resultado = window.open(direccion, "ventana_aviso","width=500,height=360,scrollbars=yes, left=0, top=0");
}
function enviar(formulario)
{
            document.getElementById("texto_alerta").style.visibility="visible";
			formulario.action ="predictivo_escuelas.asp";
			formulario.submit();
}
</script>
