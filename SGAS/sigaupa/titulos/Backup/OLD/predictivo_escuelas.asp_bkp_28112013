<%
Server.ScriptTimeOut = 150000
set pagina = new CPagina
pagina.Titulo = "Predictivo por Escuelas"
'---------------------------------------------------------------------------------------------------
set conexion = new CConexion
conexion.Inicializar "upacifico"

set errores = new CErrores

set negocio = new CNegocio
negocio.Inicializa conexion

periodo = negocio.obtenerPeriodoAcademico("Planificacion")
peri = negocio.obtenerPeriodoAcademico("CLASES18")
anos_ccod = conexion.consultaUno("Select anos_ccod from periodos_academicos where cast(peri_ccod as varchar)='"&periodo&"'")

'---------------------------------------------------------------------------------------------------
sede_ccod   =   request.QueryString("a[0][sede_ccod]")
carr_ccod   =   request.QueryString("a[0][carr_ccod]")
jorn_ccod   =   request.QueryString("a[0][jorn_ccod]")
'response.Write("estado "&estado_prestamo)
'--------------------------------------------------------------------------
set botonera = new CFormulario
botonera.Carga_Parametros "predictivo_escuelas.xml", "botonera"
'--------------------------------------------------------------------------
set formulario = new CFormulario
formulario.Carga_Parametros "predictivo_escuelas.xml", "listado_asignaturas"
formulario.Inicializar conexion 
'-------------------------------------------Seleccionar asignatura para equivalencia de una lista sin escribir su código-----
'-----------------------------------------------------------msandoval 19-02-2005---------------------------------------------
	set f_filtros = new cFormulario
	f_filtros.carga_parametros "predictivo_escuelas.xml", "buscador"
	f_filtros.inicializar conexion
	consulta="Select '"&carr_ccod&"' as carr_ccod, '"&sede_ccod&"' as sede_ccod, '"&jorn_ccod&"' as jorn_ccod"
	f_filtros.consultar consulta
	consulta = " select distinct b.sede_ccod, c.sede_tdesc, e.carr_ccod, e.carr_tdesc, f.jorn_ccod, f.jorn_tdesc  " & vbCrLf & _
			   " from alumnos a (nolock), ofertas_academicas b, sedes c, especialidades d, carreras e, jornadas f  " & vbCrLf & _
			   " where a.ofer_ncorr=b.ofer_ncorr and b.sede_ccod=c.sede_ccod  " & vbCrLf & _
			   " and b.espe_ccod=d.espe_ccod and d.carr_ccod=e.carr_ccod  " & vbCrLf & _
			   " and b.jorn_ccod=f.jorn_ccod  " & vbCrLf & _
			   " and a.emat_ccod in (1)  " & vbCrLf & _
			   " order by sede_tdesc, carr_tdesc, jorn_tdesc  "

	f_filtros.inicializaListaDependiente "filtros", consulta
	f_filtros.siguiente
'-----------------------------------------------------------------------------------------------------------------
sede = conexion.consultaUno("select sede_tdesc from sedes where cast(sede_ccod as varchar)='"&sede_ccod&"'")
carrera = conexion.consultaUno("select carr_tdesc from carreras where cast(carr_ccod as varchar)='"&carr_ccod&"'")
jornada = conexion.consultaUno("select jorn_tdesc from jornadas where cast(jorn_ccod as varchar)='"&jorn_ccod&"'")


set f_candidatos = new cFormulario
f_candidatos.carga_parametros "tabla_vacia.xml", "tabla"
f_candidatos.inicializar conexion
c_datos = " select *,(total_ramos_malla - total_ramos_aprobados_o_en_curso) as restantes   " & vbCrLf & _
          " from   " & vbCrLf & _
		  " (   " & vbCrLf & _
		  "    select sede, carrera, jornada,espe_tdesc as especialidad,ttt.plan_ccod,plan_tdesc as plan_estudio,pers_ncorr,     " & vbCrLf & _
		  "    rut, nombres,ap_paterno, ap_materno, ap_paterno + ' ' + ap_materno + ' ' + nombres as nombre_completo,   " & vbCrLf & _
		  "    ano_ingreso_carrera, ultimo_estado, ultimo_periodo, egresado,titulado,es_cae,   " & vbCrLf & _
		  "    (select count(*) from malla_curricular tr where tr.plan_ccod = table1.ultimo_plan   " & vbCrLf & _
		  "     and isnull(tr.mall_npermiso,0) = 0 ) as total_ramos_malla,   " & vbCrLf & _
		  "    (select count(*) from malla_curricular tr where tr.plan_ccod = table1.ultimo_plan   " & vbCrLf & _
		  "     and isnull(tr.mall_npermiso,0) = 0    " & vbCrLf & _
		  "     and isnull(protic.estado_ramo_alumno(table1.pers_ncorr,tr.asig_ccod,table1.carr_ccod,tr.plan_ccod,'"&peri&"'),'') <> '') as total_ramos_aprobados_o_en_curso,   " & vbCrLf & _
		  "     telefono,celular, email_particular,   " & vbCrLf & _
		  "     (select top 1 protic.trunc(fecha_cae) from detalles_titulacion_carrera ttt where ttt.pers_ncorr=table1.pers_ncorr and ttt.carr_ccod=table1.carr_ccod order by informar_cae desc ) as fecha_cae,  " & vbCrLf & _
		  "     (select top 1 observaciones_cae from detalles_titulacion_carrera ttt where ttt.pers_ncorr=table1.pers_ncorr and ttt.carr_ccod=table1.carr_ccod order by informar_cae desc ) as observaciones_cae,  " & vbCrLf & _
		  "     (select top 1 case isnull(informar_cae,3) when 0 then 'NO' when 1 then 'SI' else '' end from detalles_titulacion_carrera ttt where ttt.pers_ncorr=table1.pers_ncorr and ttt.carr_ccod=table1.carr_ccod order by informar_cae desc ) as informar_cae  " & vbCrLf & _
		  "    from   " & vbCrLf & _
		  "    (   " & vbCrLf & _
		  "    select distinct sede_tdesc as sede, e.carr_ccod, carr_tdesc as carrera, jorn_tdesc as jornada,   " & vbCrLf & _
		  "    g.pers_ncorr,cast(g.pers_nrut as varchar)+'-'+g.pers_xdv as rut, g.pers_tnombre as nombres,   " & vbCrLf & _
		  "    g.pers_tape_paterno as ap_paterno, g.pers_tape_materno as ap_materno,   " & vbCrLf & _
		  "    g.pers_tfono as telefono, g.pers_tcelular as celular, lower(g.pers_temail) as email_particular,   " & vbCrLf & _
		  "    protic.ano_ingreso_carrera_egresa2(a.pers_ncorr,e.carr_ccod) as ano_ingreso_carrera,   " & vbCrLf & _
		  "    (select top 1 plan_ccod   " & vbCrLf & _
		  "         from alumnos tt (nolock), ofertas_academicas t2, especialidades t3   " & vbCrLf & _
		  "         where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "         and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod   " & vbCrLf & _
		  "         and tt.emat_ccod = 1 order by t2.peri_ccod desc) as ultimo_plan,   " & vbCrLf & _
		  "    (select top 1 emat_tdesc   " & vbCrLf & _
		  "         from alumnos tt (nolock), ofertas_academicas t2, especialidades t3, estados_matriculas t4   " & vbCrLf & _
		  "         where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "         and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod and tt.emat_ccod=t4.emat_ccod   " & vbCrLf & _
		  "         order by t2.peri_ccod desc, tt.audi_tusuario desc) as ultimo_estado,   " & vbCrLf & _
		  "    (select top 1 peri_tdesc   " & vbCrLf & _
		  "         from alumnos tt (nolock), ofertas_academicas t2, especialidades t3, periodos_academicos t4   " & vbCrLf & _
		  "         where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "         and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod and t2.peri_ccod=t4.peri_ccod   " & vbCrLf & _
		  "         order by t2.peri_ccod desc, tt.audi_tusuario desc) as ultimo_periodo,        " & vbCrLf & _
		  "    (select case count(*) when 0 then 'NO' else 'SI' end from alumnos tt (nolock), ofertas_academicas t2, especialidades t3   " & vbCrLf & _
		  "                    where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "                    and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod   " & vbCrLf & _
		  "                    and tt.emat_ccod in (4) ) as egresado,  " & vbCrLf & _
		   "    (select case count(*) when 0 then 'NO' else 'SI' end from alumnos tt (nolock), ofertas_academicas t2, especialidades t3   " & vbCrLf & _
		  "                    where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "                    and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod   " & vbCrLf & _
		  "                    and tt.emat_ccod in (8) ) as titulado,  " & vbCrLf & _		  
		  "   isnull((select top 1 'SI en '+ cast(ttt.anos_ccod as varchar) from ufe_alumnos_cae ttt where esca_ccod=1 and ttt.rut = g.pers_nrut order by ttt.anos_ccod desc),'NO') as es_cae " & vbCrLf & _
		  "    from alumnos a (nolock), ofertas_academicas b, sedes c, especialidades d, carreras e,   " & vbCrLf & _
		  "    jornadas f, personas g  (nolock), periodos_academicos h  " & vbCrLf & _
		  "    where a.ofer_ncorr=b.ofer_ncorr and b.sede_ccod=c.sede_ccod   " & vbCrLf & _
		  "    and b.espe_ccod=d.espe_ccod and d.carr_ccod=e.carr_ccod   " & vbCrLf & _
		  "    and b.jorn_ccod=f.jorn_ccod   " & vbCrLf & _
		  "    and a.pers_ncorr=g.pers_ncorr and b.peri_ccod = h.peri_ccod  " & vbCrLf & _
		  "    --and not exists (select 1 from alumnos tt (nolock), ofertas_academicas t2, especialidades t3   " & vbCrLf & _
		  "    --                where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "    --                and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod   " & vbCrLf & _
		  "    --                and tt.emat_ccod in (8) )   " & vbCrLf & _
		  "    and (select count(*)   " & vbCrLf & _
		  "         from alumnos tt (nolock), ofertas_academicas t2, especialidades t3   " & vbCrLf & _
		  "         where tt.ofer_ncorr=t2.ofer_ncorr and t2.espe_ccod=t3.espe_ccod   " & vbCrLf & _
		  "         and tt.pers_ncorr=a.pers_ncorr and t3.carr_ccod=e.carr_ccod   " & vbCrLf & _ 
		  "         and tt.emat_ccod = 1 and isnull(tt.alum_nmatricula,0) <> 7777 ) >= 2     " & vbCrLf & _              
		  "    and cast(b.sede_ccod as varchar)='"&sede_ccod&"'   " & vbCrLf & _
		  "    and cast(b.jorn_ccod as varchar)='"&jorn_ccod&"'   " & vbCrLf & _
		  "    and cast(d.carr_ccod as varchar)='"&carr_ccod&"'   " & vbCrLf & _
		  "    )table1, planes_estudio ttt, especialidades tt2   " & vbCrLf & _
		  "    where table1.ultimo_plan=ttt.plan_ccod   " & vbCrLf & _
		  "    and ttt.espe_ccod = tt2.espe_ccod   " & vbCrLf & _
		  " )table2   " & vbCrLf & _
		  " where (total_ramos_malla - total_ramos_aprobados_o_en_curso) <= 5   " & vbCrLf & _
		  " order by sede, carrera, jornada, (total_ramos_malla - total_ramos_aprobados_o_en_curso) asc"

f_candidatos.consultar c_datos
'response.Write("<pre>"&c_datos&"</pre>")

