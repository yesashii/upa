<%
set pagina = new CPagina
pagina.Titulo = "mantener cajeros"
set errores = new CErrores
'---------------------------------------------------------------------------------------------------
set conexion = new CConexion
conexion.Inicializar "upacifico"

set negocio = new CNegocio
negocio.Inicializa conexion
'-----------------------------------------------------------------------
Periodo = negocio.ObtenerPeriodoAcademico("POSTULACION")
'---------------------------------------------------------------------------------------------------
set botonera = new CFormulario
botonera.Carga_Parametros "Mant_Cajeros.xml", "botonera"
'---------------------------------------------------------------------------------------------------
 sede = request.querystring("busqueda[0][sede_ccod]")
 rut_cajero = request.querystring("busqueda[0][pers_nrut]")
 rut_cajero_digito = request.querystring("busqueda[0][pers_xdv]")
 
 set f_busqueda = new CFormulario
 f_busqueda.Carga_Parametros "Mant_Cajeros.xml", "busqueda_cajeros"
 f_busqueda.Inicializar conexion
 f_busqueda.Consultar "select '' from dual"
 f_busqueda.Siguiente

 f_busqueda.AgregaCampoCons "sede_ccod", sede
 f_busqueda.AgregaCampoCons "pers_nrut", rut_cajero
 f_busqueda.AgregaCampoCons "pers_xdv", rut_cajero_digito

'----------------------------------------------------------------------------------

set f_cajeros = new CFormulario
f_cajeros.Carga_Parametros "Mant_Cajeros.xml", "f_cajeros"
f_cajeros.Inicializar conexion
consulta = "SELECT a.caje_ccod, a.sede_ccod, b.sede_tdesc, a.caje_cestado, c.pers_nrut, "&_ 
                  "a.pers_ncorr, obtener_rut(a.pers_ncorr) as rut, obtener_nombre_completo (a.pers_ncorr) as nombre "&_
           "FROM cajeros a, sedes b, personas c "&_
           "WHERE a.sede_ccod = b.sede_ccod "&_
             "AND a.pers_ncorr = c.pers_ncorr "
             if rut_cajero <> "" then
			   consulta = consulta &  " AND c.pers_nrut = '" & rut_cajero & "' " 
			 end if
             if sede <> "" then
			   consulta = consulta &  " AND a.sede_ccod = '" & sede & "' " 
			 end if
			 consulta = consulta &  " ORDER BY nombre"

'response.Write(consulta)

 if Request.QueryString <> "" then
	  f_cajeros.consultar consulta
  else
	f_cajeros.consultar "select '' from dual where 1 = 2"
	f_cajeros.AgregaParam "mensajeError", "Ingrese criterio de busqueda"
  end if

 
'----------------------------------------------------------------------- 
%>


