		  <salida>anos_ccod</salida>
		  <filtro>anos_ccod between year(getdate())-1 and year(getdate())+1</filtro>
		  <permiso>LECTURA</permiso>
		  <deshabilitado>false</deshabilitado>
		  <id>TO-N</id>		  
	</campos>
	<campos nombre="mes_ccod">
		  <tipo>SELECT</tipo>
		  <destino>meses</destino> 
		  <union>mes_ccod</union>
		  <salida>mes_tdesc</salida>
		  <permiso>LECTURA</permiso>
		  <anulable>false</anulable>
		  <mensajeNulo>Seleccione Mes</mensajeNulo>
		  <deshabilitado>false</deshabilitado>
