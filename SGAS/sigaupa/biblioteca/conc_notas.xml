         <descripcion>Semestre</descripcion>
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <destino>(select peri_ccod, cast(anos_ccod as varchar)+ ' - ' + cast(plec_ccod as varchar) + 'Nº Semestre ' as desc_periodo from periodos_academicos)t</destino>
