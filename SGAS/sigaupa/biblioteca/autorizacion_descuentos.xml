<Parametros>
   <parametro formulario="botonera">
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>ValidaFormBusqueda();</funcionValidacion>
      </botones>
      <botones nombre="guardar">
         <tipoTexto>GUARDAR</tipoTexto>
         <accion>ACTUALIZAR</accion>
         <formulario>edicion</formulario>
         <url>proc_autorizacion_descuentos.asp</url>
      </botones>
      <botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>navegar</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
   </parametro>
   <parametro formulario="busqueda" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-N</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <id>LN-N</id>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
      </campos>
      <campos nombre="post_ncorr">
         <tipo>SELECT</tipo>
		 <destino>(Select ) a</destino>
		 <permiso>LECTURAESCRITURA</permiso>
		 <union>post_ncorr</union>
		 <salida>carrera</salida>
		 <columnas>2</columnas>
		 <anulable>false</anulable>
		 <mensajeNulo>Seleccione una carrera</mensajeNulo>
		 <mensajeVacio>No hay carreras</mensajeVacio>
		 <estilo>anchofijo</estilo>
		 <id>TO-S</id>
      </campos>
   </parametro>
   <parametro paginaEdicion="edicion_descuento.asp?post_ncorr=%post_ncorr%&amp;ofer_ncorr=%ofer_ncorr%&amp;stde_ccod=%stde_ccod%" nuevaVentanaAlto="310" formulario="descuentos" nuevaVentanaAncho="500" resumen="TRUE" mensajeError="No hay descuentos para este postulante." seleccionarTodo="TRUE" editar="TRUE" anchoTabla="100%" nuevaVentana="TRUE" variable="descuentos">
      <tablas nombre="sdescuentos" accion="M"/>
      <campos nombre="post_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="stde_ccod">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="ofer_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="stde_tdesc">
         <descripcion>Descuento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="sdes_nporc_matricula">
         <descripcion>% Desc. Matrícula</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
		 <decimales>3</decimales>
         <formato>DECIMAL</formato>
         <resumen>SUMA_JC</resumen>
      </campos>
      <campos nombre="sdes_nporc_colegiatura">
         <descripcion>% Desc. Arancel</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
         <formato>DECIMAL</formato>
         <resumen>SUMA_JC</resumen>
      </campos>
      <campos nombre="sdes_mmatricula">
         <descripcion>Desc. Matrícula</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
         <alineamiento>RIGHT</alineamiento>
         <resumen>SUMA_JC</resumen>
      </campos>
      <campos nombre="sdes_mcolegiatura">
         <descripcion>Desc. Arancel</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
         <alineamiento>RIGHT</alineamiento>
         <resumen>SUMA_JC</resumen>
      </campos>
      <campos nombre="subtotal">
         <descripcion>Subtotal</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
         <alineamiento>RIGHT</alineamiento>
         <resumen>SUMA_JC</resumen>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="esde_ccod">
         <descripcion>Estado</descripcion>
         <tipo>SELECT</tipo>
         <destino>sestados_descuentos</destino>
         <union>esde_ccod</union>
         <salida>esde_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
   </parametro>
</Parametros>
