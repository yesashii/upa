<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
   <parametro formulario="busqueda" variable="busqueda">
	  <campos nombre="peri_ccod">
		  <tipo>SELECT</tipo>
		  <destino>(select peri_ccod,peri_tdesc from periodos_academicos where anos_ccod > 2005 and peri_ccod not in (232,233))a</destino> 
		  <union>peri_ccod</union>
		  <salida>peri_tdesc</salida>
		  <orden>peri_ccod desc</orden>	  
		  <permiso>LECTURAESCRITURA</permiso>
		  <anulable>true</anulable>
		  <mensajeNulo>Período consulta</mensajeNulo>
		  <mensajeVacio>Período consulta</mensajeVacio>
		  <id>TO-N</id>
      </campos>
   </parametro>
   <parametro formulario="botonera">
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
      </botones>
      <botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
	  <botones nombre="excel">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Excel</texto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
      </botones>
   </parametro>
   <parametro formulario="asignaturas" anchoTabla="100%" variable="asignatura" maxLista="25" nroRegistros="25"
   mensajeError="No existen asignaturas mal cerradas para el período consultado." >
      <campos nombre="sede">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Sede</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="carrera">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Carrera</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
	  <campos nombre="jornada">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Jornada</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
	  <campos nombre="cod_asignatura">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Cód. Asignatura</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
	  <campos nombre="asignatura">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Asignatura</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="sección">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Sección</descripcion>
         <alineamiento>CENTER</alineamiento>
      </campos>
   </parametro>
 </Parametros>
