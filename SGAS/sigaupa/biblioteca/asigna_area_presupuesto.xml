<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
   <parametro formulario="botonera">

	   <botones nombre="buscar2">
  		<tipoTexto>BUSCAR</tipoTexto> 
  		<accion>JAVASCRIPT</accion> 
  		<funcion>enviar();</funcion> 
  	  </botones>
	  <botones nombre="nuevo">
	    <texto>Nuevo</texto>
        <tipoTexto>Definir</tipoTexto>
  		<accion>NAVEGAR</accion> 
  		<url>javascript:nuevo_centro_costo();</url> 
      </botones>
      <botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
	  <botones nombre="guardar">
         <tipoTexto>Guardar</tipoTexto>
         <accion>Guardar</accion>
		 <formulario>buscador</formulario>
         <url>proc_asignar_area_presupuesto.asp</url>
      </botones>
	  <botones nombre="cerrar">
			<texto>Cerrar</texto>
			<tipoTexto>Definir</tipoTexto>
			<accion>NAVEGAR</accion> 
			<url>javascript:CerrarActualizar();</url> 
      </botones>
   </parametro>
   
   <parametro formulario="busqueda" variable="busca">
   		<campos nombre="area_ccod">
			  <tipo>SELECT</tipo>
			  <destino>presupuesto_upa.protic.area_presupuestal</destino> 
			  <union>area_ccod</union>
			  <salida>area_tdesc</salida>
			  <permiso>LECTURAESCRITURA</permiso>
			  <anulable>true</anulable>
			  <orden>area_tdesc</orden>
			  <mensajeNulo>Seleccione un area Presupuestal</mensajeNulo>
			  <mensajeVacio>Areas presupuestales</mensajeVacio>
			  <id>TO-N</id>
	    </campos>
	   	<campos nombre="pers_nrut">
			<tipo>INPUT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<caracteres>10</caracteres>
			<maxCaracteres>8</maxCaracteres>
			<id>NU-N</id>
		</campos>
		<campos nombre="pers_xdv">
			<tipo>INPUT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<caracteres>1</caracteres>
			<maxCaracteres>1</maxCaracteres>
			<script>onBlur="this.value=this.value.toUpperCase();"</script>
			<id>LN-N</id>
		</campos>
	    
   </parametro>
   
</Parametros>

