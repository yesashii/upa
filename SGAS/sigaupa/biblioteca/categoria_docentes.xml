	nuevaVentanaAncho="556" nuevaVentanaAlto="280"
	variable="categorias" nroRegistros="10" maxLista="10">
	 <tablas nombre="tipos_categoria" accion="B"/>
     <campos nombre="TCAT_CCOD">
	    <descripcion>Codigo</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
		<ancho>50</ancho>
	 </campos>
	 <campos nombre="TCAT_tdesc">
	    <descripcion>Nombre de categoria</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>	 
	 <campos nombre="TCAT_VALOR">
	    <descripcion>Valor de categoria</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="ANOS_CCOD">
	    <descripcion>AÃ‘O</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="jdoc_ccod">
	    <descripcion>Jerarquia</descripcion>
		<destino>jerarquias_docentes</destino>
		<union>jdoc_ccod</union>
	    <salida>jdoc_tdesc</salida>
	    <permiso>LECTURA</permiso>
	    <anulable>true</anulable>
	    <orden>jdoc_tdesc</orden>
	    <mensajeNulo>Seleccione Jerarquia</mensajeNulo>
	 </campos>		 	 
   </parametro>
   
 <parametro formulario="f1_edicion" variable="categorias">
   <tablas nombre="tipos_categoria" accion="M"/>
   <campos nombre="TCAT_CCOD">
      <tipo>INPUT</tipo>
	  <permiso>OCULTO</permiso>
   </campos>
   <campos nombre="TCAT_tdesc">
      <tipo>INPUT</tipo>
	  <permiso>LECTURAESCRITURA</permiso>
	  <caracteres>50</caracteres>
	  <maxCaracteres>100</maxCaracteres>
	  <id>TO-N</id>
   </campos>
   <campos nombre="TCAT_VALOR">
      <tipo>INPUT</tipo>
	  <permiso>LECTURAESCRITURA</permiso>
	  <caracteres>50</caracteres>
	  <maxCaracteres>100</maxCaracteres>
	  <id>TO-N</id>
   </campos>
  <campos nombre="jdoc_ccod" >
      <tipo>SELECT</tipo> 
      <destino>jerarquias_docentes</destino>
      <union>jdoc_ccod</union> 
      <salida>jdoc_tdesc</salida> 
      <permiso>LECTURAESCRITURA</permiso> 
	  <orden>jdoc_ccod</orden>
	  <mensajeNulo>Seleccione una Jerarquia</mensajeNulo>
	  <id>TO-N</id>
	</campos>    
   </parametro>
  
  <parametro formulario="botonera">
  <botones nombre="eliminar"> 
      <tipoTexto>ELIMINAR</tipoTexto>
