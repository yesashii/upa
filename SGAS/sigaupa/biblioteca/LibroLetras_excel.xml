  <parametro formulario="busqueda_documentos" variable="busqueda">      
  <campos nombre="mes">
      <tipo>SELECT</tipo>
	  <destino>(select 01 AS mes, 'ENERO' as descripcion union select 02 AS mes, 'FEBRERO' as descripcion union select 03 AS mes, 'MARZO' as descripcion union select 04 AS mes, 'ABRIL' as descripcion union select 05 AS mes, 'MAYO' as descripcion union select 06 AS mes, 'JUNIO' as descripcion union select 07 AS mes, 'JULIO' as descripcion union select 08 AS mes, 'AGOSTO' as descripcion union select 09 AS mes, 'SEPTIEMBRE' as descripcion union select 10 AS mes, 'OCTUBRE' as descripcion union select 11 AS mes, 'NOVIEMBRE' as descripcion union select 12 AS mes, 'DICIEMBRE' as descripcion ) a</destino> 
	  <union>mes</union>
	  <salida>descripcion</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <orden>a.mes</orden>
	  <mensajeNulo>Todos</mensajeNulo>
  </campos>
  <campos nombre="ano">
      <tipo>SELECT</tipo>
	  <destino>COMPROMISOS</destino> 
	  <union>ano</union>
	  <salida>ano2</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <orden>a.ano2</orden>
	  <mensajeNulo>Todos</mensajeNulo>
  </campos>
 </parametro>



