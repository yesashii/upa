<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
<parametro formulario="botonera">
   <botones nombre="salir"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>NAVEGAR</accion>
		<url>../COMPRAS/AUTORIZACION_GIROS.ASP</url>
	</botones>
	<botones nombre="guardar">
		 <tipoTexto>GUARDAR</tipoTexto>
		 <accion>GUARDAR</accion>
		 <url>devolucion_alumno_proc.asp?prueba=12</url>
		 <formulario>datos</formulario>
	</botones>
	<botones nombre="guardarenviar">
		 <tipoTexto>DEFINIR</tipoTexto>
		<texto>Enviar a VB</texto>
		 <accion>GUARDAR</accion>
		 <url>devolucion_alumno_proc.asp?prueba=0</url>
		 <formulario>datos</formulario>
		 <funcionValidacion>GuardarEnviar();</funcionValidacion>
	</botones>
	<botones nombre="imprimir"> 
		<tipoTexto>DEFINIR</tipoTexto>
		<texto>Imprimir</texto>
		<accion>Navegar</accion>
		<url>javascript:ImprimirDevolucionAlumno();</url>
	</botones>		
</parametro>

<parametro formulario="datos_funcionario" variable="datos" clave="dalu_ncorr">
 		<tablas nombre="ocag_devolucion_alumno" accion="M"/>
	<campos nombre="dalu_ncorr">
		<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="vibo_ccod">
		<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="cod_pre">
		 <tipo>SELECT</tipo>
		 <destino>ocag_codigos_presupuesto</destino>
		 <union>cod_pre</union>
		 <salida>valor</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
	</campos>		
	<campos nombre="pers_tnombre">
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
		<caracteres>25</caracteres>
		<maxCaracteres>40</maxCaracteres>
		<id>TO-N</id>
    </campos>
	<campos nombre="v_nombre">
		 <tipo>INPUT</tipo>
		 <permiso>LECTURA</permiso>
		 <id>TO-N</id>
	</campos>			
	<campos nombre="pers_nrut">
		<tipo>INPUT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<caracteres>10</caracteres>
		<maxCaracteres>8</maxCaracteres>
		<script>onBlur="genera_digito(this.value);" onkeypress="return solonumero(event)"</script>
		<id>NU-N</id>
    </campos>
	<campos nombre="pers_xdv">
		<tipo>INPUT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<caracteres>2</caracteres>
		<maxCaracteres>1</maxCaracteres>
		<id>TO-N</id>
	</campos>
	<campos nombre="dalu_fpago">
		 <tipo>INPUT</tipo>
		 <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>15</caracteres>
		 <maxCaracteres>10</maxCaracteres>
		 <id>FE-N</id>
	</campos>		
	<campos nombre="dalu_mmonto_pesos">
		 <tipo>INPUT</tipo>
		 <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>20</caracteres>
		 <maxCaracteres>20</maxCaracteres>
		 <script>onkeypress="return solonumero(event)"</script>
		 <id>NU-N</id>
	</campos>
	<campos nombre="carrera_alu">
		<tipo>INPUT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<caracteres>60</caracteres>
		<maxCaracteres>60</maxCaracteres>
		<id>TO-N</id>
    </campos>				
	<campos nombre="mes_ccod">
		 <tipo>SELECT</tipo>
		 <destino>MESES</destino>
		 <union>mes_ccod</union>
		 <salida>mes_tdesc</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
		 <orden>mes_ccod</orden>
	</campos>
	<campos nombre="anos_ccod">
		 <tipo>SELECT</tipo>
		 <destino>anos</destino>
		 <union>anos_ccod</union>
		 <salida>anos_ccod</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
		 <filtro>anos_ccod >= year(getdate())</filtro>		 
	</campos>
	<campos nombre="tdev_ccod">
		 <tipo>SELECT</tipo>
		 <destino>ocag_tipo_devolucion</destino>
		 <union>tdev_ccod</union>
		 <salida>tdev_tdesc</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
		 <filtro>etde_ccod = 0</filtro>	
		 <orden>tdev_tdesc</orden>
	</campos>	
	<campos nombre="dalu_tmotivo">
		 <tipo>TEXTAREA</tipo>
		 <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>100</caracteres>
		 <filas>8</filas>
	</campos>
	<campos nombre="tmon_ccod">
		 <tipo>SELECT</tipo>
		 <destino>ocag_tipo_moneda</destino>
		 <union>tmon_ccod</union>
		 <salida>tmon_tdesc</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
		 <orden>tmon_ccod</orden>
	</campos>				
</parametro> 

<parametro formulario="buscador_area" variable="datos">
	<campos nombre="area_ccod">
			 <tipo>SELECT</tipo>
			 <destino>presupuesto_upa.protic.area_presupuestal</destino>
			 <union>area_ccod</union>
			 <salida>area_tdesc</salida>
			 <permiso>LECTURAESCRITURA</permiso>
			 <anulable>false</anulable>
			 <orden>area_tdesc</orden>
	</campos>
</parametro>

<parametro formulario="codigo_presupuesto" variable="datos">
	<campos nombre="cod_pre">
		 <tipo>SELECT</tipo>
		 <destino>presupuesto_upa.protic.presupuesto_upa_2010</destino>
		 <union>cod_pre</union>
		 <salida>valor</salida>
		 <permiso>LECTURAESCRITURA</permiso>
		 <anulable>false</anulable>
	</campos>
</parametro> 

<parametro formulario="busqueda" variable="b">
      <listas nombre="busqueda">
         <item clave="CARR_CURSO" valor="CARR_CURSO"/>
         <item clave="CCOS_CCOD" valor="CCOS_TCOMPUESTO"/>
      </listas>
</parametro>

</Parametros>
