<Parametros>
  <parametro formulario="botonera">
   <botones nombre="salir"> 
      <tipoTexto>SALIR</tipoTexto>
	  <accion>NAVEGAR</accion>
	  <url>../lanzadera/lanzadera.asp</url>
	</botones>  
   <botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto>
	  <accion>BUSCAR</accion>
	  <formulario>buscador</formulario>
	  <soloUnClick>TRUE</soloUnClick>	  
	</botones>  
   </parametro>   
  <parametro formulario="f_documento" variable="documentos" anchoTabla="100%"
  eliminar="FALSE" nroRegistros="10" maxLista="10"  
   mensajeError="No se encontraron documentos...">
  <campos nombre="c_ding_ndocto">
      <tipo>INPUT</tipo>
	  <permiso>OCULTO</permiso>	  
   </campos>
   <campos nombre="comp_ndocto">
      <descripcion>N Contrato</descripcion>
	  <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
   </campos>
   <campos nombre="tcom_tdesc">
   <descripcion>Tipo Compromiso</descripcion>
     <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
   </campos>
   <campos nombre="ding_ndocto">
   <descripcion>N Docto</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
   </campos>
   <campos nombre="ingr_ncorr">
   <descripcion>N Ingreso</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>OCULTO</permiso>
   </campos>
  <campos nombre="ting_ccod">
   <descripcion>Tipo Docto</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>OCULTO</permiso>	  
   </campos>
   <campos nombre="ting_tdesc">
   <descripcion>Tipo Docto</descripcion>
     <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
  </campos>
  <campos nombre="ding_mdetalle">
  <descripcion>Monto</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
	  <formato>MONEDA</formato>
	  <alineamiento>RIGHT</alineamiento>
	</campos>
	<campos nombre="edin_ccod">
	<descripcion>Estado</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>OCULTO</permiso>
	</campos>
	<campos nombre="estado">
	<descripcion>Estado</descripcion>
      <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
	  <alineamiento>LEFT</alineamiento>
	</campos>
	<campos nombre="repa_ncorr">
	<descripcion>N Repactacion</descripcion>
     <tipo>INPUT</tipo>
	  <permiso>LECTURA</permiso>
	</campos>
  </parametro>
    
  <parametro formulario="busqueda_documentos" variable="busqueda">
  <campos nombre="ding_ndocto">
     <tipo>INPUT</tipo>
     <permiso>LECTURAESCRITURA</permiso>
     <caracteres>10</caracteres>
     <maxCaracteres>8</maxCaracteres>
     <id>NU-N</id>
    </campos>
   <campos nombre="edin_ccod">
      <tipo>SELECT</tipo>
	  <destino>estados_detalle_ingresos</destino> 
	  <union>edin_ccod</union>
	  <salida>edin_tdesc</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Todos</mensajeNulo>
	 </campos>
	 <campos nombre="ting_ccod">
      <tipo>SELECT</tipo>
	  <destino>tipos_ingresos</destino> 
	  <union>ting_ccod</union>
	  <filtro>ting_ccod in (4,3)</filtro>
	  <salida>ting_tdesc</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Todos</mensajeNulo>
	</campos>
	 <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-S</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-S</id>
      </campos>
	  <campos nombre="code_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-S</id>
      </campos>
      <campos nombre="code_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-S</id>
      </campos>
  </parametro>
 </Parametros>
