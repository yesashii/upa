   <parametro formulario="fselect" variable="salas">
   <campos nombre="sala_ccod">
      <tipo>SELECT</tipo>
	  <destino>(select a.sala_ccod,b.sede_tdesc + ': '+a.sala_tdesc as sala_tdesc 
				from salas a, sedes b 
				where a.sede_ccod=b.sede_ccod and a.sede_ccod in (1,4,8)
				and a.sala_ccod in (175,102,161,176,30,32,31,29,43,274,68))b
	  </destino> 
	  <union>sala_ccod</union>
	  <salida>sala_tdesc</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Seleccione Sala</mensajeNulo>
	  <orden>sala_tdesc</orden>
	  <id>TO-N</id>
   </campos>
   </parametro>
