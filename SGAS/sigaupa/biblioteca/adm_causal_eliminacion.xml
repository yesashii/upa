<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
   <parametro formulario="formu_causal" anchoTabla="100%" 
   colorResaltado="#CCCCCC"  mensajeError="El rut consultado no presenta causal de eliminación registrada"
   colorSeleccionado="#CCCCCC" eliminar="FALSE" seleccionarTodo="FALSE"
    editar="FALSE" 	paginaEdicion="" nuevaVentana="FALSE" variable="dp">
     <campos nombre="alumno">
	    <descripcion>Datos Alumno</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="sexo">
	    <descripcion>Causal</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos> 
   </parametro> 
   <parametro formulario="botonera">
   	 <botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto>
	  <accion>BUSCAR</accion>
	  <formulario>buscador</formulario>
	   <funcionValidacion>Validar();</funcionValidacion>
	</botones>
   	<botones nombre="salir"> 
      <tipoTexto>SALIR</tipoTexto>
	  <accion>NAVEGAR</accion>
	  <url>../lanzadera/lanzadera.asp</url>	 
	</botones>	
	<botones nombre="autorizar"> 
        <tipoTexto>DEFINIR</tipoTexto>
		<texto>Autorizar Ingreso del Alumno a Laboratorios</texto>
	    <accion>NAVEGAR</accion>
	    <url>autorizar_ingreso.asp</url>
	    <formulario>autorizar</formulario>
	</botones>
	 <botones nombre="imprimir_alumno"> 
        <tipoTexto>DEFINIR</tipoTexto>
	    <texto>Imprimir Ficha</texto>
		<accion>AGREGAR</accion>
		<target>_blank</target>
	 </botones>
	 <botones nombre="eliminar"> 
        <tipoTexto>DEFINIR</tipoTexto>
		<texto>Quitar causal de eliminación</texto>
	    <accion>GUARDAR</accion>
	    <url>adm_causal_eliminacion_quitar.asp</url>
	    <formulario>edicion_causal</formulario>
	</botones>
	<botones nombre="nueva"> 
        <tipoTexto>DEFINIR</tipoTexto>
		<texto>Agregar bloqueo</texto>
	    <accion>AGREGAR</accion>
	    <posicionX>100</posicionX>
	    <posicionY>50</posicionY>
	    <ancho>500</ancho>
	    <alto>250</alto>	   
	 </botones>
	 <botones nombre="guardar_nueva"> 
        <tipoTexto>GUARDAR</tipoTexto>
	    <accion>GUARDAR</accion>
	    <url>adm_causal_eliminacion_agregar_proc.asp</url>
	    <formulario>edicion</formulario>
	</botones>
		 <botones nombre="cancelar"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>CERRAR</accion>
	 </botones>	
   </parametro>
  <parametro formulario="busqueda_usuarios_nuevo" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
		 <script>onBlur="genera_digito(this.value);"</script>
         <id>NU-S</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-S</id>
      </campos>
</parametro>
</Parametros>