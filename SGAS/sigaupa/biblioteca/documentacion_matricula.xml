      <campos nombre="doma_ccod"/>
   </parametro>
   
   <parametro formulario="f_forma_mat" variable="documentos">
   <campos nombre="tfma_ccod">
      <tipo>SELECT</tipo>
	  <destino>TIPOS_FORMA_MATRICULA where habilitado='S'</destino> 
	  <union>tfma_ccod</union>
	  <salida>tfma_TDESC</salida>
      <orden>orden asc </orden>	
	  <permiso>LECTURAESCRITURA</permiso>
	  <id>TO-N</id>
   </campos>
  </parametro>
  
   
