         <destino>(select distinct cast(b.hora_ccod as varchar)+' - '+ cast(datepart(hour,a.hora_hinicio) as varchar)+':'+ cast(datepart(minute,a.hora_hinicio) as varchar) +' - '+ cast(datepart(hour,a.hora_htermino) as varchar)+':'+ cast(datepart(minute,a.hora_htermino) as varchar) as Bloque_Horario,b.hora_ccod, a.sede_ccod from horarios_sedes a, horarios b where a.hora_ccod=b.hora_ccod ) c</destino>
         <union>hora_ccod</union>
         <codigo>hora_ccod</codigo>
		 <orden>c.hora_ccod</orden>
         <salida>Bloque_Horario</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <columnas>4</columnas>
      </campos>
      <campos nombre="ssec_ncorr">
         <tipo>HIDDEN</tipo>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
      <campos nombre="secc_ccod">
         <tipo>HIDDEN</tipo>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
      <campos nombre="dias_ccod">
         <tipo>select</tipo>
         <destino>dias_semana</destino>
         <union>dias_ccod</union>
         <salida>dias_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <columnas>4</columnas>
      </campos>
	  <campos nombre="sala_ccod">
         <tipo>select</tipo>
         <destino>(select a.sala_ccod, a.sede_ccod, esal_ccod, a.sala_ncupo
       , c.sede_tdesc+ ' : ' + cast(Left(tsal_tdesc,1) as varchar) + '-' + cast(sala_ciso as varchar)+ '('  + cast(sala_ncupo as varchar)+ ')' as sala
    from salas a, tipos_sala b,sedes c
    where a.tsal_ccod=b.tsal_ccod and a.sede_ccod=c.sede_ccod
	AND a.sala_ccod in (30,32,31,29,43,274,65,25,175,176,102,161,85,133,167,266,336)
	) a</destino>
         <union>sala_ccod</union>
         <salida>sala</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <orden>sala asc </orden>
         <columnas>4</columnas>
		 <anulable>TRUE</anulable>
		 <mensajeVacio>SIN ASIGNAR</mensajeVacio>
		 <mensajeNulo>SIN ASIGNAR</mensajeNulo>
      </campos>
	  <campos nombre="bloq_ayudantia">
		 <tipo>select</tipo>
         <destino>(select 0 as bloq_ayudantia,'Cátedra' as descripcion 
					union 
					select 1 as bloq_ayudantia, 'Ayudantía' as descripcion
					union 
					select 2 as bloq_ayudantia, 'Laboratorio' as descripcion
					union
					select 3 as bloq_ayudantia, 'Terreno' as descripcion
					union
					select 4 as bloq_ayudantia, 'Elearning' as descripcion)a
		 </destino>
         <union>bloq_ayudantia</union>
		 <salida>descripcion</salida>
		 <orden>bloq_ayudantia</orden>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
