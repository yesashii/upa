<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>

<parametro formulario="botonera">
      <botones nombre="lanzadera">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
      <botones nombre="excel_facturas">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Excel</texto>
         <accion>BUSCAR</accion>
         <url>libro_facturas_excel.asp</url>
         <formulario>buscador</formulario>
      </botones>
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>Validar();</funcionValidacion>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
   </parametro>

   <parametro formulario="f_facturas" clave="envi_ncorr" variable="facturas" nroRegistros="50" maxLista="10" 
   anchoTabla="100%" mensajeError="No se encontraron facturas...">
       <campos nombre="tipo_factura">
         <descripcion>Tipo</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
	  <campos nombre="num_factura">
         <descripcion>Factura</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <ancho>50</ancho>
         <alineamiento>CENTER</alineamiento>
      </campos>
	  <campos nombre="total_factura">
         <descripcion>Monto</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>RIGHT</alineamiento>
         <formato>MONEDA</formato>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="estado">
         <descripcion>Estado</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="fecha_factura">
         <descripcion>F. Emision</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="rut_beneficiario">
         <descripcion>Rut beneficiario</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="nombre_beneficiario">
         <descripcion>Nombre Beneficiario</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
  	  <campos nombre="comprobante">
         <descripcion>Nº Comprobante</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
	  <campos nombre="caja">
         <descripcion>Nº Caja</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
 </parametro>

 <parametro formulario="busqueda_facturas" variable="busqueda">
      <campos nombre="sede_ccod">
         <tipo>SELECT</tipo>
         <destino>sedes</destino>
         <union>sede_ccod</union>
         <salida>sede_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>true</anulable>
         <mensajeNulo>Todas las Sedes</mensajeNulo>
		 <filtro>sede_ccod not in (3,5,6)</filtro>
      </campos>
      <campos nombre="inicio">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>12</caracteres>
         <maxCaracteres>10</maxCaracteres>
         <id>FE-S</id>
      </campos>
      <campos nombre="termino">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>12</caracteres>
         <maxCaracteres>10</maxCaracteres>
         <id>FE-S</id>
      </campos>
      <campos nombre="fact_nfactura">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>12</caracteres>
         <maxCaracteres>10</maxCaracteres>
         <id>NU-S</id>
      </campos>
      <campos nombre="efac_ccod">
         <tipo>SELECT</tipo>
         <destino>estados_facturas</destino>
         <union>efac_ccod</union>
         <salida>efac_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>true</anulable>
         <mensajeNulo>Todos los Estados</mensajeNulo>
      </campos>
	  <campos nombre="tfac_ccod">
         <tipo>SELECT</tipo>
         <destino>tipos_facturas</destino>
         <union>tfac_ccod</union>
         <salida>tfac_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>true</anulable>
         <mensajeNulo>Todos los tipos</mensajeNulo>
      </campos>
   </parametro>
   
</Parametros>


