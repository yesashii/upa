<Parametros>
   <parametro formulario="botonera">
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>ValidaFormBusqueda();</funcionValidacion>
      </botones>
      <botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>navegar</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
	  <botones nombre="calcular">
         <tipoTexto>DEFINIR</tipoTexto>
		 <texto>Calcular</texto>
         <accion>JAVASCRIPT</accion>
         <funcion>calcular()</funcion>
      </botones>
	  <botones nombre="guardar">
         <tipoTexto>DEFINIR</tipoTexto>
		 <texto>Guardar</texto>
         <accion>JAVASCRIPT</accion>
         <funcion>enviar()</funcion>
      </botones>
	  <botones nombre="salir2">
         <tipoTexto>DEFINIR</tipoTexto>
		 <texto>Salir</texto>
         <accion>JAVASCRIPT</accion>
         <funcion>salir()</funcion>
      </botones>
	  <botones nombre="imprimir">
         <tipoTexto>DEFINIR</tipoTexto>
		 <texto>Imprimir</texto>
         <accion>JAVASCRIPT</accion>
         <funcion>imprimir()</funcion>
      </botones>
   </parametro>
   <parametro formulario="busqueda" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-N</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-N</id>
      </campos>
   </parametro>
   <parametro formulario="documentos" variable="documentos" anchoTabla="98%" mensajeError="No ha enviado su postulaciÃ³n.">
      <tablas nombre="documentos_postulantes" accion="M"/>
      <campos nombre="pers_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="doma_ccod">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="doma_tdesc">
         <descripcion>Documento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="bentregado">
         <descripcion>Entregado</descripcion>
         <tipo>BOLEANO</tipo>
         <objeto>CHECKBOX</objeto>
         <permiso>LECTURAESCRITURA</permiso>
         <valorVerdadero>S</valorVerdadero>
         <valorFalso>N</valorFalso>
         <alineamiento>CENTER</alineamiento>
      </campos>
   </parametro>
   <parametro formulario="agrega_convalidacion" variable="a">
      <tablas nombre="cargos_convalidacion" accion="M"/>
      <campos nombre="jorn_ncorr"/>
      <campos nombre="carcon_ncorr"/>
      <campos nombre="carcon_ncantidad_asig"/>
      <campos nombre="carcon_total"/>
	  <campos nombre="ofer_ncorr"/>
   </parametro>
   
    <parametro formulario="pase_escolar" variable="a">
   		<campos nombre="alumno">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Nombre</descripcion>
         <alineamiento>CENTER</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>		
		<campos nombre="rut">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>R.U.T.</descripcion>
         <alineamiento>CENTER</alineamiento>
        </campos>
		<campos nombre="carrera">
		 <tipo>SELECT</tipo>
         <destino>carreras</destino>
         <union>carr_ccod</union>
         <salida>carr_tdesc</salida>
		 <permiso>LECTURAEscritura</permiso>
		 </campos>
   </parametro>
</Parametros>
