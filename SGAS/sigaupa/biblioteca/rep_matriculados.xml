<Parametros>
	<parametro formulario="sede" variable="busqueda">
   <campos nombre="sede_ccod">
      <tipo>SELECT</tipo>
	  <destino>sedes</destino> 
	  <union>sede_ccod</union>
	  <salida>sede_tdesc</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Todas las Sedes</mensajeNulo>
	 </campos>
	 <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-S</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-S</id>
      </campos>
</parametro>
<parametro formulario="matriculados" nroRegistros="10" maxLista="10" 
		mensajeError="No hay alumnos matriculados" anchoTabla="98%" 
		colorResaltado="#DCDCB8" colorBase="#F1F1E4" colorSeleccionado="99FFFF" 
		editar="false" eliminar="false" agregar="false" nuevaVentana="false"
		 >
		<campos nombre="rut">
			<tipo>IMPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Rut</descripcion>
			<alineamiento>Right</alineamiento>
		</campos>
		<campos nombre="alumno">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Nombre</descripcion>
			<alineamiento>LEFT</alineamiento>
		</campos>
		<campos nombre="espe_tdesc">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Especialidad</descripcion>
			<alineamiento>CENTER</alineamiento>
		</campos>
		<campos nombre="carrera">
			<tipo>IMPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Carrera</descripcion>
			<alineamiento>CENTER</alineamiento>
		</campos>
		<campos nombre="alum_fmatricula">
			<tipo>IMPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Fecha Matricula</descripcion>
			<alineamiento>CENTER</alineamiento>
			<tipoDato>FECHA</tipoDato>
		</campos>
	</parametro>
	 <parametro formulario="btn_matriculados">
		<botones nombre="buscar"> 
		  <tipoTexto>BUSCAR</tipoTexto>
		  <formulario>buscador</formulario>
		  <accion>buscar</accion>
		</botones> 
		<botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
       </botones>
	 </parametro>
</Parametros>


