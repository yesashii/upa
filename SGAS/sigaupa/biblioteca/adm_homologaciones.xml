<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
 <parametro formulario="btn_adm_homologaciones">
    
	<botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto>
	  <accion>JAVASCRIPT</accion>
	  <funcion>enviar(document.buscador)</funcion>	 
	</botones> 
    
	  <botones nombre="eliminar">
         <tipoTexto>ELIMINAR</tipoTexto>
         <accion>ELIMINAR</accion>
         <url>proc_eliminar_homologaciones.asp</url>
         <formulario>edicion</formulario>
      </botones> 
    
	<botones nombre="AGREGAR"> 
      <tipoTexto>AGREGAR</tipoTexto>
	  <accion>JAVASCRIPT</accion>
	  <funcion>agrega_carrera(document.edicion)</funcion>
	</botones> 
    
	  <botones nombre="SALIR">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>	
	
	  <botones nombre="excel">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Excel</texto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
     </botones> 
	
	 <botones nombre="excel_general">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Excel Datos Generales</texto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
     </botones> 	
 
 </parametro>
 
 
  <parametro formulario="btn_agregar_carrera">
    <botones nombre="AGREGAR"> 
      <tipoTexto>AGREGAR</tipoTexto>
	  <accion>JAVASCRIPT</accion>
	  <funcion>agregar(document.editar)</funcion>
	</botones> 
    <botones nombre="GUARDAR"> 
      <tipoTexto>GUARDAR</tipoTexto>
	  <accion>JAVASCRIPT</accion>
	  <funcion>agregar(document.editar)</funcion>
	</botones> 	
  <botones nombre="SALIR"> 
      <tipoTexto>SALIR</tipoTexto>
	  <accion>JAVASCRIPT</accion>
	  <funcion>salir(document.edicion)</funcion>
	</botones> 
 </parametro>


 	
	<parametro colorSeleccionado="99FFFF" variable="em" colorResaltado="#DCDCB8" paginaEdicion="" 
		 formulario="tabla_valores" eliminar="true"  colorBase="#F1F1E4" editar="true"
		 mensajeError="No Existen Datos Para Los Parámetros De Búsqueda." 
			anchoTabla="98%"  nuevaVentana="true">
		
			
		<campos nombre="CARR_TDESC">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <descripcion>Carrera</descripcion>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-N</id>
        </campos>
		
		<campos nombre="carr_ccod">
         <tipo>select</tipo>
		 <union>carr_ccod</union>
         <salida>carr_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <mensajeNulo>--Seleccione una Carrera--</mensajeNulo>
		 <anulable>TRUE</anulable>
		 <id>TO-N</id>
       </campos>	  
		
		<campos nombre="car_ing_ncorr">
         <tipo>select</tipo>
         <destino>(select car_ing_ncorr ,nom_carrera_ing +' (cod:'+cod_carrera_ing+')' as cod_carrera_ing from ufe_carreras_ingresa) a </destino>
         <union>car_ing_ncorr</union>
         <salida>cod_carrera_ing</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <mensajeNulo>SIN ASIGNAR</mensajeNulo>
         <mensajeVacio>SIN ASIGNAR</mensajeVacio>
		 <anulable>TRUE</anulable>
         <columnas>4</columnas>
       </campos>
		
		<campos nombre="car_min_ncorr">
         <tipo>select</tipo>
         <destino>(select car_min_ncorr ,nom_carrera_min +' (cod:'+cod_carrera_min+')' as cod_carrera_min from ufe_carreras_mineduc) b</destino>
         <union>car_min_ncorr</union>
         <salida>cod_carrera_min</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <mensajeNulo>SIN ASIGNAR</mensajeNulo>
         <mensajeVacio>SIN ASIGNAR</mensajeVacio>
		 <anulable>TRUE</anulable>
         <columnas>4</columnas>
       </campos>
	   <campos nombre="car_duracion_semestres">
         <tipo>input</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>3</caracteres>
		 <maxCaracteres>2</maxCaracteres>
		 <id>NU-S</id>
       </campos>	
	   
	   <campos nombre="car_duracion_anos">
         <tipo>input</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>3</caracteres>
		  <maxCaracteres>2</maxCaracteres>
		 <id>NU-S</id>
       </campos>
	   <campos nombre="ttie_ccod">
         <tipo>select</tipo>
         <destino>(select ttie_ccod ,ttie_tdesc +' (cod:'+cast(ttie_ccod as varchar)+')' as ttie_tdesc from ufe_tipo_titulo_ies) b</destino>
         <union>ttie_ccod</union>
         <salida>ttie_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <mensajeNulo>Seleccione un tipo</mensajeNulo>
         <mensajeVacio>''</mensajeVacio>
		 <anulable>TRUE</anulable>
         <columnas>4</columnas>
		 <id>NU-S</id>
       </campos>	
	 </parametro>	
		

  <parametro formulario="mantiene_homologaciones" variable="em">
      <tablas nombre="ufe_carreras_homologadas" accion="M"/>
   </parametro>

  <parametro formulario="eliminar_homologaciones" variable="em">
      <tablas nombre="ufe_carreras_homologadas" accion="B"/>
   </parametro>
  
  <parametro formulario="datos_extras" variable="em">
	    <listas nombre="lBusqueda">
			<item clave="acar_ccod" valor="acar_tdesc" />
			<item clave="saca_ccod" valor="saca_tdesc" />
		</listas>
		<campos nombre="acar_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
		<campos nombre="saca_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
	</parametro>
	 
	 
	 	<parametro colorSeleccionado="99FFFF" colorResaltado="#DCDCB8"
	 paginaEdicion="agregar_homologaciones.asp?carr_ccod=%carr_ccod%" 
	 formulario="tabla" eliminar="true" clave="eliminar" nroRegistros="10" maxLista="10"
	 mensajeError="No Existen Datos Para Los Parámetros De Búsqueda." 
	 colorBase="#F1F1E4" editar="true" 	anchoTabla="98%" variable="em" nuevaVentana="true" nuevaVentanaAlto="400" nuevaVentanaAncho="750" >
	
	<tablas nombre="carreras" accion="B"/>
	
	<campos nombre="carr_ccod">
			<tipo>INPUT</tipo>
			<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="carr_tdesc">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Carrera</descripcion>
			<alineamiento>left</alineamiento>
	</campos>
	<campos nombre="cod_carrera_ing">
			<tipo>IMPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Codigo Ing</descripcion>
			<alineamiento>left</alineamiento>
	</campos>
	<campos nombre="cod_carrera_min">
			<tipo>IMPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Codigo Min</descripcion>
			<alineamiento>left</alineamiento>
	</campos>
	
	</parametro>


</Parametros>