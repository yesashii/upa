   <parametro formulario="f_letras" clave="envi_ncorr" variable="letras" nroRegistros="50" maxLista="10" 
   anchoTabla="100%" mensajeError="No se encontraron letras...">
      <campos nombre="ding_ndocto">
         <descripcion>Letra</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <ancho>50</ancho>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="edin_tdesc">
         <descripcion>Estado</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="ingr_fpago">
         <descripcion>F. Emision</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="ding_fdocto">
         <descripcion>F. Vencimiento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="ding_mdocto">
         <descripcion>Monto</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>RIGHT</alineamiento>
         <formato>MONEDA</formato>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="rut_alumno">
         <descripcion>Rut Alumno</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="rut_apoderado">
         <descripcion>Rut Apoderado</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
      <campos nombre="institucion">
         <descripcion>Institucion</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
      </campos>
	  	  <campos nombre="comprobante">
         <descripcion>Nº Comprobante</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
         <nowrap>TRUE</nowrap>
