		 <tipo>SELECT</tipo>
		 <destino>MESES</destino>
		 <union>mes_ccod</union>
		 <salida>mes_tdesc</salida>
		 <permiso>LECTURA</permiso>
		 <anulable>false</anulable>
		 <orden>mes_ccod</orden>
	</campos>
	<campos nombre="anos_ccod">
		 <tipo>SELECT</tipo>
		 <destino>anos</destino>
		 <union>anos_ccod</union>
		 <salida>anos_ccod</salida>
		 <permiso>LECTURA</permiso>
		 <anulable>false</anulable>
		 <orden>anos_ccod</orden>
	</campos>
	<campos nombre="ccos_ncorr">
		  <tipo>SELECT</tipo>
		  <destino>(select b.pers_nrut,a.ccos_ncorr,a.ccos_tcodigo as ccos_tcompuesto,ccos_tdesc  
from ocag_centro_costo a, ocag_permisos_centro_costo b 
where a.ccos_tcodigo=b.ccos_tcodigo ) as tabla</destino>
		  <union>ccos_ncorr</union>
		  <salida>ccos_tcompuesto</salida>
		  <permiso>LECTURA</permiso>
		  <deshabilitado>true</deshabilitado>
		  <anulable>false</anulable>
		  <id>TO-N</id>		  
	</campos>
