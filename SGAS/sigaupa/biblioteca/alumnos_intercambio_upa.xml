         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <anulable>true</anulable>
	    <mensajeNulo>Todos</mensajeNulo>
		<mensajeVacio>''</mensajeVacio>
		 <destino>paises</destino>
		 <filtro>pais_ccod in (select pais_ccod from ciudades_extranjeras a ,universidad_ciudad b ,datos_convenio c where a.ciex_ccod=b.ciex_ccod and b.unci_ncorr=c.unci_ncorr)</filtro>
		  <union>pais_ccod</union>
         <salida>pais_tdesc</salida>
		 <orden>pais_tdesc</orden>
		 <script>OnChange="cambiar_pais();"</script>
		 <id>NU-S</id>
