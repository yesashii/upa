<Parametros>
   <parametro formulario="botonera">
      <botones nombre="anterior">
         <tipoTexto>ANTERIOR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>adm_salidas_carrera.asp</url>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
      <botones nombre="agregar">
         <tipoTexto>AGREGAR</tipoTexto>
         <accion>AGREGAR</accion>
         <alto>500</alto>
         <ancho>800</ancho>
         <url>adm_asignaturas_salidas_carrera_agregar.asp</url>
         <scroll>TRUE</scroll>
      </botones>
      <botones nombre="eliminar">
         <tipoTexto>ELIMINAR</tipoTexto>
         <accion>ELIMINAR</accion>
         <url>proc_eliminar_asignaturas_salida_carrera.asp</url>
         <formulario>edicion</formulario>
      </botones>
      <botones nombre="aceptar">
         <tipoTexto>ACEPTAR</tipoTexto>
         <accion>GUARDAR</accion>
         <url>proc_adm_asignaturas_salidas_carrera_agregar.asp</url>
         <formulario>edicion</formulario>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
	  <botones nombre="cerrar">
         <tipoTexto>CANCELAR</tipoTexto>
         <accion>CERRAR</accion>
      </botones>
	  <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
		 <soloUnClick>TRUE</soloUnClick>
      </botones>
   </parametro>
   <parametro formulario="encabezado" anchoTabla="100%" columnas="2">
      <campos nombre="carr_tdesc">
         <descripcion>Carrera</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="espe_tdesc">
         <descripcion>Especialidad</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="plan_tdesc">
         <descripcion>Plan</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="tsca_tdesc">
         <descripcion>Tipo de Salida</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_tdesc">
         <descripcion>Salida</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_npond_asignaturas">
         <descripcion>% Pond. Asignaturas</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="total">
         <descripcion>Total Asignaturas</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
   </parametro>
   <parametro formulario="encabezado_otros" anchoTabla="100%" columnas="2">
      <campos nombre="carr_tdesc">
         <descripcion>Carrera</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="tsca_tdesc">
         <descripcion>Tipo de Salida</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_tdesc">
         <descripcion>Salida</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_npond_asignaturas">
         <descripcion>% Pond. Asignaturas</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="total">
         <descripcion>Total Asignaturas</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
   </parametro>
   <parametro formulario="asignaturas" anchoTabla="100%" mensajeError="No se han agregado asignaturas." eliminar="TRUE" clave="mall_ccod" seleccionarTodo="TRUE" variable="as" nroRegistros="10" maxLista="10" enumerarLista="TRUE">
      <tablas nombre="asignaturas_salidas_carrera" accion="B"/>
      <campos nombre="nive_ccod">
         <descripcion>Nivel</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="asig_ccod">
         <descripcion>Código</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="asig_tdesc">
         <descripcion>Asignatura</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="carr_tdesc">
         <descripcion>Carrera</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
   </parametro>
   <parametro formulario="asignaturas_malla" anchoTabla="100%" eliminar="TRUE" clave="mall_ccod" seleccionarTodo="TRUE" 
   variable="as" mensajeError="No existen más asignaturas asociadas al plan de estudios." nroRegistros="55" maxLista="10">
      <tablas nombre="asignaturas_salidas_carrera" accion="M"/>
      <campos nombre="nive_ccod">
         <descripcion>Nivel</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="asig_ccod">
         <descripcion>Código</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="asig_tdesc">
         <descripcion>Nombre Asignatura</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="saca_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
   </parametro>
   <parametro formulario="busqueda" variable="b">
      <listas nombre="busqueda">
         <item clave="espe_ccod" valor="espe_tdesc"/>
         <item clave="plan_ccod" valor="plan_tdesc"/>
      </listas>
   </parametro>
   <parametro formulario="busqueda_minors" variable="b">
      <listas nombre="busqueda">
         <item clave="carr_ccod" valor="carr_tdesc"/>
		 <item clave="espe_ccod" valor="espe_tdesc"/>
         <item clave="plan_ccod" valor="plan_tdesc"/>
      </listas>
   </parametro>
</Parametros>
