		<mensajeVacio>''</mensajeVacio>
		 <destino>facultades</destino>
		 <filtro>facu_ccod in (1,2,3,4,8)</filtro>
		  <union>facu_ccod</union>
         <salida>facu_tdesc</salida>
		 <orden>facu_tdesc asc</orden>
		 <script>onChange="bloquearcampo(this.value,this.name)"</script>
      </campos>
	</parametro>
	
