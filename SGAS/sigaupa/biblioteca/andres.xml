      <campos nombre="inst_ccod">
         <tipo>SELECT</tipo>
         <destino>instituciones</destino>
         <union>inst_ccod</union>
         <salida>inst_trazon_social</salida>
         <orden>inst_ccod asc </orden>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>TRUE</anulable>
         <mensajeNulo>Seleccione institución</mensajeNulo>
      </campos>
      <campos nombre="tcom_ccod">
         <tipo>SELECT</tipo>
         <destino>tipos_compromisos</destino>
         <filtro>tcom_bcargo = 'S'</filtro>
         <union>tcom_ccod</union>
         <salida>tcom_tdesc</salida>
         <orden>tcom_ccod asc </orden>
         <permiso>LECTURAESCRITURA</permiso>
         <script>onChange="RecargarComboTiposDetalle(this.form, this.value)"</script>
      </campos>
      <campos nombre="comp_fdocto">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <tipoDato>FECHA</tipoDato>
      </campos>
      <campos nombre="tdet_ccod">
         <tipo>SELECT</tipo>
         <destino>tipos_detalle</destino>
         <filtro>(tcom_ccod=8) AND (tdet_mvalor_unitario &lt;&gt; 0)</filtro>
         <union>tdet_ccod</union>
         <salida>tdet_tdesc</salida>
         <orden>tdet_ccod asc </orden>
         <permiso>LECTURAESCRITURA</permiso>
         <script>onChange="CambiarCombo(this.value, this.form)"</script>
         <anulable>TRUE</anulable>
      </campos>
      <campos nombre="deta_ncantidad">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
