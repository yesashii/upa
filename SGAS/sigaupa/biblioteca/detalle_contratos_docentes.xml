     <botones nombre="lanzadera"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>NAVEGAR</accion>
		<url>../lanzadera/lanzadera.asp</url>
	 </botones>
	 <botones nombre="cancelar"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>CERRAR</accion>
	 </botones>
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>ValidaBusqueda();</funcionValidacion>
      </botones>
	<botones nombre="guardar"> 
      <tipoTexto>DEFINIR</tipoTexto>
	  <texto>Guardar</texto>
	  <accion>GUARDAR</accion>
	  <formulario>edicion</formulario>
	  <url>proc_guardar_anexos.asp</url>
	  <funcionValidacion>Guardar_Anexos(document.edicion);</funcionValidacion>
	</botones>
	<botones nombre="anular"> 
      <tipoTexto>DEFINIR</tipoTexto>
	  <texto>Anular</texto>
	  <accion>GUARDAR</accion>
	  <formulario>edicion</formulario>
	  <url>proc_anular_anexos.asp</url>
	  <funcionValidacion>Anula_Anexos(document.edicion);</funcionValidacion>
	</botones>
	<botones nombre="anular_contrato"> 
      <tipoTexto>DEFINIR</tipoTexto>
	  <texto>Anular Contrato</texto>
	  <accion>GUARDAR</accion>
	  <formulario>contrato</formulario>
	  <url>proc_anular_contrato.asp</url>
	  <funcionValidacion>Anula_Contrato(document.contrato);</funcionValidacion>
	</botones>
	<botones nombre="guardar_contrato"> 
      <tipoTexto>DEFINIR</tipoTexto>
	  <texto>Guarda Contrato</texto>
	  <accion>GUARDAR</accion>
	  <formulario>contrato</formulario>
	  <url>proc_guardar_contrato.asp</url>
	  <funcionValidacion>Guardar_Contrato(document.contrato);</funcionValidacion>
	</botones>
