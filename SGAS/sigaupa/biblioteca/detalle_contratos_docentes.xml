         <tipoTexto>ANTERIOR</tipoTexto>
         <accion>JAVASCRIPT</accion>
         <funcion>history.back()</funcion>
    </botones>	  
  </parametro>
  
  
  <parametro formulario="contrato_creados" variable="cdoc" anchoTabla="98%" 
