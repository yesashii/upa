<parametro formulario="toma_carga" variable="d">
		<campos nombre="secc_ccod">
			<tipo>SELECT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<union>secc_ccod</union>
			<salida>horario</salida>
			<columnas>2</columnas>
			<anulable>true</anulable>
			<mensajeNulo>Sin sección asignada</mensajeNulo>
			<mensajeVacio>No hay secciones disponibles</mensajeVacio>
			<estilo>anchofijo</estilo>
			<id>TO-N</id>
		</campos>
		<campos nombre="asignatura">
			<tipo>SELECT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<union>mall_ccod</union>
			<salida>asignatura</salida>
			<columnas>2</columnas>
			<anulable>true</anulable>
			<mensajeNulo>Seleccione asignatura equivalente</mensajeNulo>
			<mensajeVacio>No hay asignaturas disponibles</mensajeVacio>
			<estilo>anchofijo</estilo>
			<id>TO-N</id>
		</campos>
	</parametro>
	<parametro formulario="buscador" variable="a">
	    <listas nombre="lBusqueda">
			<item clave="carr_ccod" valor="carr_tdesc" />
			<item clave="asig_ccod" valor="asig_tdesc" />
		</listas>
		<campos nombre="carr_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
		<campos nombre="asig_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
	</parametro>
	<parametro formulario="buscador_optativos" variable="a">
	    <listas nombre="lBusqueda">
			<item clave="asig_tdesc" valor="asig_tdesc" />
			<item clave="secc_ccod" valor="secc_tdesc" />
		</listas>
		<campos nombre="asig_tdesc">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
		<campos nombre="secc_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
	</parametro>
	<parametro formulario="combo_carreras" variable="ch">
		<campos nombre="matr_ncorr">
			<tipo>SELECT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<union>parametro</union>
			<salida>salida</salida>
			<script>onchange='javascript:dibujar(document.temporal);'</script>			
			<id>TO-N</id>
		</campos>
	</parametro>
	<parametro formulario="tabla_Op_deportivos" variable="d" clave="secc_ccod" mensajeError="No hay optativos deportivos para el alumno."
		 maxLista="10" nroRegistros="10" anchoTabla="99%" colorResaltado="#DCDCB8" colorBase="#F1F1E4" colorSeleccionado="99FFFF" eliminar="true" >
		<campos nombre="matr_ncorr">
			<tipo>INPUT</tipo>
			<permiso>OCULTO</permiso>
		</campos>
		<campos nombre="asignatura">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Asignatura</descripcion>
			<alineamiento>CENTER</alineamiento>
		</campos>
		<campos nombre="horario">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Horario</descripcion>
			<alineamiento>CENTER</alineamiento>
		</campos>
		<campos nombre="afecta">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Afecta promedio</descripcion>
			<alineamiento>CENTER</alineamiento>
		</campos>
	</parametro>
	<parametro formulario="afecta_promedio" variable="d">
	 <campos nombre="carg_afecta_promedio">
         <tipo>BOLEANO</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <objeto>RADIO</objeto>
         <valorVerdadero>S</valorVerdadero>
         <valorFalso>N</valorFalso>
         <afirmacion>Sí</afirmacion>
         <negacion>No</negacion>
		 <id>TO-S</id>
      </campos>
	 </parametro>
