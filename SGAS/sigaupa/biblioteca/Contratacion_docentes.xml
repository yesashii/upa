<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
  <parametro formulario="botonera">
     <botones nombre="lanzadera"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>NAVEGAR</accion>
		<url>../lanzadera/lanzadera.asp</url>
	 </botones>
	 <botones nombre="cancelar"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>CERRAR</accion>
	 </botones>
	 <botones nombre="buscar"> 
        <tipoTexto>BUSCAR</tipoTexto>
	    <accion>BUSCAR</accion>
	    <formulario>buscador</formulario>
		<soloUnClick>TRUE</soloUnClick>
	 </botones>
	 <botones nombre="nueva"> 
        <tipoTexto>AGREGAR</tipoTexto>
	    <accion>AGREGAR</accion>
	    <posicionX>100</posicionX>
	    <posicionY>50</posicionY>
	    <ancho>270</ancho>
	    <alto>170</alto>	   
		<url>Habilitacion_Agregar.asp</url>		
	 </botones>
	 <botones nombre="guardar_nueva"> 
        <tipoTexto>GUARDAR</tipoTexto>
	    <accion>GUARDAR</accion>
	    <url>Proc_Planes_Agregar.asp</url>
	    <formulario>edicion</formulario>
	</botones>
	<botones nombre="eliminar"> 
      <tipoTexto>ELIMINAR</tipoTexto>
	  <accion>ELIMINAR</accion>
	  <formulario>edicion</formulario>
	  <url>Habilitacion_Eliminar.asp</url>
	</botones> 
	<botones nombre="Calcular"> 
       <tipoTexto>DEFINIR</tipoTexto>
	    <texto>Calcular</texto>
		<accion>AGREGAR</accion>
		<target>_blank</target>			
	</botones> 
	<botones nombre="imprimir_contrato"> 
        <tipoTexto>DEFINIR</tipoTexto>
	    <texto>Imprimir</texto>
		<accion>AGREGAR</accion>
		<target>_blank</target>			
	 </botones>		
  </parametro>
 
 <parametro formulario="f_busqueda" variable="busqueda">
   <campos nombre="carr_ccod">
      <tipo>SELECT</tipo>
	  <destino>carreras</destino> 
	  <union>carr_ccod</union>
	  <salida>CARR_TDESC</salida>
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <orden>carr_tdesc</orden>
	  <mensajeNulo>Seleccione Carrera</mensajeNulo>
	  <mensajeVacio>Seleccione Carrera</mensajeVacio>
	  <id>TO-N</id>
   </campos> 
  <campos nombre="espe_ccod" >
      <tipo>SELECT</tipo> 
      <destino>especialidades</destino>
      <union>espe_ccod</union> 
      <salida>espe_tdesc</salida> 
      <permiso>LECTURAESCRITURA</permiso> 
	  <orden>espe_tdesc</orden>
	  <mensajeNulo>Seleccione una Especialidad</mensajeNulo>
	  <mensajeVacio>Seleccione una Especialidad</mensajeVacio>
	  <id>TO-N</id>
	</campos>
  </parametro>
  
   <parametro  paginaEdicion="Habilitacion_modificar.asp?carr_ccod=%carr_ccod%&amp;pers_ncorr=%pers_ncorr%"
      nuevaVentanaAlto="400" formulario="filtro_docentes2"  nroRegistros="10"  nuevaVentanaAncho="770"
      eliminar="TRUE" clave="pers_ncorr" maxLista="10"
	  mensajeError="No hay docentes que coincidan con el criterio de busqueda"
	   seleccionarTodo="TRUE"  anchoTabla="100%" editar="TRUE"  nuevaVentana="TRUE" 
	  variable="mm" >   
      <campos nombre="pers_ncorr">
         <tipo>HIDDEN</tipo>
         <permiso>INVISIBLE</permiso>
      </campos>
      <campos nombre="sede_ccod">
         <tipo>HIDDEN</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="tdir_ccod">
         <tipo>HIDDEN</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="rut">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <id>NU-S</id>
         <descripcion>Rut Docente</descripcion>
         <maxCaracteres>3</maxCaracteres>
         <caracteres>5</caracteres>
         <etiqueta>false</etiqueta>
         <alineamiento>center</alineamiento>
      </campos>
      <campos nombre="nom">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <id>LE-S</id>
         <descripcion>Nombre del Docente</descripcion>
      </campos>
      <campos nombre="PENDIENTES">
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <id>LE-S</id>
         <descripcion>Bloques Carga Académica</descripcion>
      </campos>	  
   </parametro>  
  
   <parametro formulario="f_nuevoo" variable="m" clave="pers_ncorr">
    <tablas nombre="CARRERAS_DOCENTE" accion="m"/>	
 </parametro>   
   
   <parametro  paginaEdicion="Planes_Agregar.asp?carr_ccod=%carr_ccod%" 
      nuevaVentanaAlto="400" formulario="f_habilitacion"  nroRegistros="10"  nuevaVentanaAncho="770"
      eliminar="TRUE" maxLista="10" clave="pers_ncorr"
	  mensajeError="No se encontraron Docentes Habilitados" seleccionarTodo="TRUE" editar="TRUE"  anchoTabla="100%"  nuevaVentana="TRUE" 
	  variable="habilitacion" >
      <tablas nombre="CARRERAS_DOCENTE" accion="B"/>
      <campos nombre="pers_ncorr">
		<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>				
	 </campos>
      <tablas nombre="CARRERAS_DOCENTE" accion="B"/>
      <campos nombre="carr_ccod">
		<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>				
	 </campos>	 
   </parametro>
   
   <parametro formulario="f_nuevo" variable="habilitacion">
    <tablas nombre="CARRERAS_DOCENTE" accion="M"/>
	 <campos nombre="pers_ncorr">
	 	<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	 </campos>
	 <campos nombre="carr_ccod">
	 	<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	 </campos>
	 <campos nombre="pers_tcoduas">
	 	<tipo>INPUT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<caracteres>30</caracteres>
	    <maxCaracteres>20</maxCaracteres>
	    <id>TO-N</id>		
	 </campos>	 
 </parametro>
 </Parametros>
