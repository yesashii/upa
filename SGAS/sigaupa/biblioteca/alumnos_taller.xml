   <parametro formulario="busqueda" variable="b" columnas="3" >
          <campos nombre="pers_nrut" >
         <descripcion>Nº Documento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <id>NU-S</id>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
      
      </campos>
	   <campos nombre="pers_xdv" >
         <descripcion>Nº Documento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <id>TO-S</id>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
      
      </campos>
	    <campos nombre="tasi_ncorr" >
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <anulable>true</anulable>
	    <mensajeNulo>Seleccione un Taller</mensajeNulo>
		<mensajeVacio>''</mensajeVacio>
		 <destino>talleres_sicologia </destino>
		 <union>tasi_ncorr</union>
         <salida>tasi_tdesc</salida>
		 <orden>tasi_ncorr asc</orden>
		 <id>NU-S</id>
      </campos>
	  <campos nombre="anos_ccod" >
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <anulable>true</anulable>
	    <mensajeNulo>Seleccione un Periodo Academico</mensajeNulo>
		<mensajeVacio>''</mensajeVacio>
		 <destino>anos</destino>
		 <filtro>anos_ccod > 2004 </filtro>
         <union>anos_ccod</union>
         <salida>anos_tdesc</salida>
		 <orden>anos_tdesc asc</orden>
		 <id>NU-N</id>
      </campos>
	  	   <campos nombre="sede_ccod" >
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <anulable>true</anulable>
	    <mensajeNulo>Todas las Sedes</mensajeNulo>
		<mensajeVacio>''</mensajeVacio>
		 <destino>sedes </destino>
		 <filtro>sede_ccod in (1,2,4,7,8)</filtro>
         <union>sede_ccod</union>
         <salida>sede_tdesc</salida>
		 
      </campos>
	  <campos nombre="carr_ccod">
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <anulable>true</anulable>
	    <mensajeNulo>Seleccione una Carrera</mensajeNulo>
		<mensajeVacio>''</mensajeVacio>
		 <destino>carreras </destino>
		 <union>carr_ccod</union>
		 <filtro>carr_ccod in (11,12,14,16,21,23,32,33,36,41,43,45,47,49,51,8,810,830,840,850,860,870,880,900,920,930,940,950,970,980,99,990)</filtro>
         <salida>carr_tdesc</salida>
		 <orden>carr_tdesc asc</orden>
		 
      </campos>
	  
