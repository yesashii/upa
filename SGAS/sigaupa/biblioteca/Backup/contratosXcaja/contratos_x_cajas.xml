   <parametro formulario="busqueda" variable="busqueda">
   	  <campos nombre="sede_ccod">
         <tipo>SELECT</tipo>
		 <destino>sedes</destino>
		 <union>sede_ccod</union>
		 <salida>sede_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
		 <anulable>True</anulable>
		 <mensajeNulo>Todas las Sedes</mensajeNulo>
		 <mensajeVacio>Elija Sede</mensajeVacio>
         <id>NU-S</id>
      </campos>
	  <campos nombre="pers_ncorr">
		<tipo>SELECT</tipo>
		<destino>(select pers_ncorr,protic.obtener_nombre(pers_ncorr,'n') as nombre from cajeros where caje_cestado not in (3) group by pers_ncorr)  a order by a.nombre</destino>
		<union>pers_ncorr</union>
		<salida>nombre</salida>
		<permiso>LECTURAESCRITURA</permiso>
		<anulable>true</anulable>
		<mensajeNulo>Seleccione Cajero</mensajeNulo>
		<id>TO-S</id>
	</campos>
	<campos nombre="periodo">
		 <tipo>BOLEANO</tipo>
         <objeto>CHECKBOX</objeto>
         <permiso>LECTURAESCRITURA</permiso>
		 <valorVerdadero>1</valorVerdadero>
		 <valorFalso>0</valorFalso>
      </campos>
	  <campos nombre="inicio">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>12</caracteres>
         <maxCaracteres>10</maxCaracteres>
         <id>FE-S</id>
      </campos>
      <campos nombre="termino">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>12</caracteres>
         <maxCaracteres>10</maxCaracteres>
         <id>FE-S</id>
      </campos>
   </parametro>
 
