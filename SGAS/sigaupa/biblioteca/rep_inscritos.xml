      <campos nombre="sede_ccod">
         <descripcion>Sede</descripcion>
         <tipo>SELECT</tipo>
         <destino>sedes</destino>
         <union>sede_ccod</union>
         <salida>sede_tdesc</salida>
         <orden>sede_tdesc asc</orden>
         <id>TO-N</id>
         <anulable>TRUE</anulable>
         <mensajeNulo>Seleccione sede</mensajeNulo>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
      <campos nombre="peri_ccod">
         <descripcion>Semestre</descripcion>
         <tipo>SELECT</tipo>
         <destino>(select peri_ccod,peri_tdesc from periodos_academicos where anos_ccod=2005)a</destino>
         <union>peri_ccod</union>
         <salida>peri_tdesc</salida>
         <orden>peri_ccod asc</orden>
         <id>TO-N</id>
         <anulable>TRUE</anulable>
         <mensajeNulo>Seleccione semestre</mensajeNulo>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
