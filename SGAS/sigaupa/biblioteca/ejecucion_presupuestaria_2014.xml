	<parametro formulario="busqueda_presupuesto" variable="busqueda">
		<campos nombre="area_ccod">
			 <tipo>SELECT</tipo>
			 <destino>presupuesto_upa.protic.area_presupuestal</destino>
			 <union>area_ccod</union>
			 <salida>area_tdesc</salida>
			 <orden>area_tdesc asc</orden>
			 <permiso>LECTURAESCRITURA</permiso>
			 <anulable>false</anulable>
		</campos>
		<campos nombre="codcaja">
			 <tipo>SELECT</tipo>
			 <destino>presupuesto_upa.protic.vis_presupuesto_anual</destino>
			 <union>cod_pre</union>
			 <salida>desc_cod_pre</salida>
			 <orden>desc_cod_pre asc</orden>
			 <permiso>LECTURAESCRITURA</permiso>
			 <anulable>true</anulable>
			 <mensajeNulo>Todos los codigos</mensajeNulo>
			 <script>onChange="CargarCodigo(this.form, this.value)"</script> 
		</campos>		
		<campos nombre="inicio">
			 <tipo>INPUT</tipo>
			 <permiso>LECTURAESCRITURA</permiso>
			 <caracteres>12</caracteres>
