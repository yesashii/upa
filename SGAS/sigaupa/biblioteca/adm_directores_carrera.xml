   </parametro>
   <parametro paginaEdicion="editar_director.asp?d[0][carr_ccod]=%carr_ccod%&amp;d[0][sede_ccod]=%sede_ccod%&amp;d[0][jorn_ccod]=%jorn_ccod%&amp;d[0][pers_nrut]=%pers_nrut%" 
   formulario="carreras" editar="TRUE" anchoTabla="100%" nuevaVentana="TRUE" nuevaVentanaAncho="600" nuevaVentanaAlto="270">
