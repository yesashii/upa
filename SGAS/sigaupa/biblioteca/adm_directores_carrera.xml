         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="nombre_director">
         <descripcion>Director</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
    </parametro>
