         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="mcaj_finicio">
         <descripcion>Fecha de Apertura</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <tipoDato>FECHA</tipoDato>
      </campos>
      <campos nombre="mcaj_mrendicion">
         <descripcion>Monto de Apertura</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
      </campos>
      <campos nombre="fecha_emision">
         <descripcion>Fecha de Emisión</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <tipoDato>FECHA</tipoDato>
      </campos>
   </parametro>
   <parametro formulario="documentos_caja" anchoTabla="100%" variable="detalle_mov_cajas" >
      <tablas nombre="detalle_mov_cajas" accion="M"/>
      <campos nombre="mcaj_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="inst_ccod">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="tdoc_ccod">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="tdoc_tdesc">
         <descripcion>&amp;nbsp;</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <ancho>80%</ancho>
      </campos>
      <campos nombre="mcaj_mtotal">
         <descripcion>Total</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <formato>MONEDA</formato>
         <alineamiento>CENTER</alineamiento>
         <caracteres>13</caracteres>
         <ancho>20%</ancho>
         <onBlur>mcaj_mtotal_blur(this);</onBlur>
      </campos>
   </parametro>
   <parametro formulario="suma" variable="suma">
      <campos nombre="total">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <formato>MONEDA</formato>
         <soloLectura>TRUE</soloLectura>
         <caracteres>20</caracteres>
      </campos>
   </parametro>
   
