   <parametro formulario="movimiento_caja" anchoTabla="100%" columnas="2" variable="movimientos_cajas">
      <tablas nombre="movimientos_cajas" accion="M"/>
      <campos nombre="mcaj_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="c_mcaj_ncorr">
         <descripcion>Folio</descripcion>
