		<campos nombre="fecha_cambio">
			 <tipo>INPUT</tipo>
			 <permiso>LECTURA</permiso>
			 <descripcion>Fecha Cambio</descripcion>
		</campos>				
		<campos nombre="solicitud">
			 <tipo>INPUT</tipo>
			 <permiso>LECTURA</permiso>
			 <descripcion>Solicitud</descripcion>
		</campos>
		<campos nombre="fecha_pago">
			 <tipo>INPUT</tipo>
			 <permiso>LECTURA</permiso>
