	  <campos nombre="edin_ccod">
         <tipo>SELECT</tipo>
         <destino>(select edin_ccod, edin_tdesc from estados_detalle_ingresos)a</destino>
         <union>edin_ccod</union>
         <salida>edin_tdesc</salida>
		 <orden>a.edin_tdesc</orden> 
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>true</anulable>
         <mensajeNulo>Seleccione Estado</mensajeNulo>
      </campos>
      <campos nombre="ting_ccod">
         <tipo>SELECT</tipo>
         <destino>tipos_ingresos</destino>
         <union>ting_ccod</union>
         <salida>ting_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>true</anulable>
         <filtro>ting_ccod IN (3,4)</filtro>
         <mensajeNulo>Seleccione tipo Documento</mensajeNulo>
		 <id>NU-N</id>
      </campos>
   </parametro>
