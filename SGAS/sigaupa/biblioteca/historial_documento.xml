<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
   <parametro formulario="botonera" >
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
      </botones>
      <botones nombre="ok" >
         <tipoTexto>GUARDAR</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
         <url>proc_pago_cheques.asp</url>
         <soloUnClick>TRUE</soloUnClick>
         <mensajeConfirmacion>¿Está seguro que desea recepcionar en caja los documentos seleccionados?</mensajeConfirmacion>
      </botones>
      <botones nombre="salir" >
         <tipoTexto>SALIR</tipoTexto>
         <accion>navegar</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
      <botones nombre="cerrar" >
         <tipoTexto>CERRAR</tipoTexto>
         <accion>CERRAR</accion>
      </botones>
   </parametro>
   <parametro formulario="busqueda" variable="b" columnas="3" >
      <campos nombre="ding_ndocto" >
         <descripcion>Nº Documento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <id>NU-N</id>
         <caracteres>15</caracteres>
         <maxCaracteres>10</maxCaracteres>
      </campos>
      <campos nombre="ting_ccod" >
         <tipo>SELECT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <destino>tipos_ingresos</destino>
         <union>ting_ccod</union>
         <salida>ting_tdesc</salida>
		 <filtro>ting_ccod in (3,4,52)</filtro>
      </campos>
   </parametro>
   
   <parametro formulario="cheques" clave="ding_ndocto" mensajeError="Ingrese Nº documento y tipo documento." 
     anchoTabla="100%" nuevaVentana="TRUE" variable="cheques" >
      <campos nombre="ding_ndocto_origen">
		<descripcion>Numero</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
	  <campos nombre="estado_origen" >
		 <descripcion>Estado origen</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="estado_destino" >
		 <descripcion>Estado destino</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="fecha_destino">
		 <descripcion>Fecha vencimiento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
		<formato>Fecha</formato>
      </campos>
      <campos nombre="ding_mdocto_destino" >
		 <descripcion>Monto</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
		 <formato>MONEDA</formato>
      </campos>
      <campos nombre="fecha_mod" >
         <descripcion>Fecha modificacion</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
		<formato>Fecha</formato>
      </campos>
      <campos nombre="envi_ncorr_destino" >
         <descripcion>deposito</descripcion>
		 <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
		 <formato>Fecha</formato>
      </campos>
      <campos  nombre="audi_tusuario_origen">
         <descripcion>Usuario Origen</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
      </campos>
      <campos  nombre="audi_tusuario_destino">
         <descripcion>Usuario Destino</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
      </campos>
	 
      <campos  nombre="nombre_cajero">
         <descripcion>Usuario que lo Ingreso</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>LEFT</alineamiento>
      </campos>
   </parametro>
   
   <parametro formulario="encabezado_cheque" anchoTabla="90%" columnas="2" separacionColumnas="50">
      <campos nombre="ding_ndocto">
         <descripcion>Número</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="banc_ccod">
         <descripcion>Banco</descripcion>
         <tipo>SELECT</tipo>
         <destino>bancos</destino>
         <union>banc_ccod</union>
         <salida>banc_tdesc</salida>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="ding_tcuenta_corriente">
         <descripcion>Cuenta Corriente</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="ding_fdocto">
         <descripcion>Fecha</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <tipoDato>FECHA</tipoDato>
      </campos>
      <campos nombre="valor_cheque">
         <descripcion>Monto</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
      </campos>
   </parametro>
   <parametro formulario="detalle_abonos_cheque" anchoTabla="100%">
      <campos nombre="ting_ccod">
         <descripcion>Tipo</descripcion>
         <tipo>SELECT</tipo>
         <destino>tipos_ingresos</destino>
         <union>ting_ccod</union>
         <salida>ting_tdesc</salida>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="ingr_nfolio_referencia">
         <descripcion>Folio</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="ingr_fpago">
         <descripcion>Fecha</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <tipoDato>FECHA</tipoDato>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="abon_mabono">
         <descripcion>Monto</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <formato>MONEDA</formato>
         <alineamiento>RIGHT</alineamiento>
      </campos>
      <campos nombre="rut">
         <descripcion>RUT Alumno</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <nowrap>TRUE</nowrap>
         <alineamiento>CENTER</alineamiento>
      </campos>
      <campos nombre="ting_ccod_documento">
         <descripcion>Docto.</descripcion>
         <tipo>SELECT</tipo>
         <destino>tipos_ingresos</destino>
         <union>ting_ccod</union>
         <salida>ting_tdesc</salida>
         <permiso>LECTURA</permiso>
      </campos>
      <campos nombre="ding_ndocto">
         <descripcion>Nº</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
   </parametro>
</Parametros>
