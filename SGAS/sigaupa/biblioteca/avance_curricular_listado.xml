   <campos nombre="ufco_ncorr">
      <tipo>SELECT</tipo>
	  <destino>(select ufco_ncorr,ufco_tdescripcion
				from ufe_comparador a
				where exists (select 1 
							  from pro_tabla_campos 
							  where table_name = a.ufco_ttabla))a</destino> 
	  <union>ufco_ncorr</union>
	  <salida>ufco_tdescripcion</salida>
      <orden>ufco_tdescripcion asc </orden>	  
