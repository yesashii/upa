	 <campos nombre="asig_ccod">
	 	<tipo>Select</tipo>
		<destino>(select distinct(a.asig_ccod),a.asig_tdesc
		from asignaturas a, carreras b, secciones c
		where cast(b.area_ccod as varchar) = '%area_ccod%'
		and b.carr_ccod=c.carr_ccod
		and c.asig_ccod=a.asig_ccod) a</destino>
