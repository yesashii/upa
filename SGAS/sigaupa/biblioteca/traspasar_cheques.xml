		<maxCaracteres>8</maxCaracteres>
		<id>NU-S</id>
	</campos>
	<campos nombre="banc_ccod">
		<tipo>SELECT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<destino>ocag_bancos_softland</destino> 
		<union>banc_ccod</union>
		<salida>banc_tdesc</salida>
		<mensajeNulo>Seleccione un Banco pagador</mensajeNulo>
		<mensajeVacio>Seleccione un Banco pagador</mensajeVacio>
		<anulable>true</anulable>
		<id>NU-S</id>
	</campos>	
</parametro>


