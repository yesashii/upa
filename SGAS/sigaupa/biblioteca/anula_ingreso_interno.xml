<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
 
      <parametro formulario="busqueda" variable="busqueda">
		  <campos nombre="ingr_nfolio_referencia">
			 <tipo>INPUT</tipo>
			 <permiso>LECTURAESCRITURA</permiso>
			 <caracteres>10</caracteres>
			 <maxCaracteres>10</maxCaracteres>
			 <id>NU-N</id>
		  </campos>
		  <campos nombre="ting_ccod">
			 <tipo>SELECT</tipo>
			 <destino>tipos_ingresos</destino>
			 <union>ting_ccod</union>
			 <salida>ting_tdesc</salida>
			 <filtro>ting_ccod in (7, 16, 17, 33, 34, 10, 8, 2,15)</filtro>
			 <orden>ting_tdesc asc</orden>
			 <permiso>LECTURAESCRITURA</permiso>
			 <anulable>TRUE</anulable>
			 <mensajeNulo>Seleccione tipo</mensajeNulo>
			 <id>TO-N</id>
		  </campos>
		  <campos nombre="mcaj_ncorr">
			 <tipo>SELECT</tipo>
			 <union>mcaj_ncorr</union>
			 <salida>cajero</salida>
			 <permiso>LECTURAESCRITURA</permiso>
			 <anulable>TRUE</anulable>
			 <mensajeNulo>Seleccione Cajero</mensajeNulo>
			 <id>TO-N</id>
		  </campos>
   </parametro>
   
