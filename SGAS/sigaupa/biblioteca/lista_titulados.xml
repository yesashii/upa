      </campos>
      <campos nombre="ntitulados">
         <descripcion>N° Titulados</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
         <alineamiento>CENTER</alineamiento>
      </campos>
   </parametro>
