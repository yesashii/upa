<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
   <parametro formulario="asignaturas" anchoTabla="100%" 
   colorResaltado="#CCCCCC"  mensajeError="No existe asignaturas tomadas para el alumno"
   colorSeleccionado="#CCCCCC" eliminar="FALSE" seleccionarTodo="FALSE"
    editar="FALSE" 	paginaEdicion="" nuevaVentana="FALSE" variable="asi">
     <campos nombre="asignatura">
	    <descripcion>Asignatura</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="seccion">
	    <descripcion>Sección</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos> 
	 <campos nombre="profesor">
	    <descripcion>Profesor</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="estado">
	    <descripcion>Evaluado</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
   </parametro>
   <parametro formulario="botonera">
   	 <botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto>
	  <accion>BUSCAR</accion>
	  <formulario>buscador</formulario>
	   <funcionValidacion>Validar();</funcionValidacion>
	</botones>
   	<botones nombre="salir"> 
      <tipoTexto>SALIR</tipoTexto>
	  <accion>NAVEGAR</accion>
	  <url>../lanzadera/lanzadera.asp</url>	 
	</botones>	
	<botones nombre="grabar"> 
        <tipoTexto>DEFINIR</tipoTexto>
		<texto>Grabar estado</texto>
	    <accion>NAVEGAR</accion>
	    <url>perdonar_ev_docente_proc.asp</url>
	    <formulario>autorizar</formulario>
	</botones>
	 <botones nombre="imprimir_alumno"> 
        <tipoTexto>DEFINIR</tipoTexto>
	    <texto>Imprimir Ficha</texto>
		<accion>AGREGAR</accion>
		<target>_blank</target>
	 </botones>	
   </parametro>
   <parametro formulario="busqueda_usuarios_nuevo" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
		 <script>onBlur="genera_digito(this.value);"</script>
         <id>NU-S</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <script>onBlur="this.value=this.value.toUpperCase();"</script>
         <id>LN-S</id>
      </campos>
   </parametro>
</Parametros>