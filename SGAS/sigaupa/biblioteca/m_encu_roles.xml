<Parametros>
 <parametro formulario="fconsulta" variable="personas">
    <tablas nombre="personas" accion="M"/>
	 <campos nombre="pers_ncorr">
	   <descripcion>Correlativo</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="pers_nrut">
	   <descripcion>Rut</descripcion>
		<tipo>INPUT</tipo>
		<caracteres>10</caracteres>
       <maxCaracteres>8</maxCaracteres>
	   <permiso>LECTURAESCRITURA</permiso>
	   <id>NU-N</id>
	 </campos>
	 <campos nombre="pers_xdv">
	   <descripcion>Digito</descripcion>
		<tipo>INPUT</tipo>
		<caracteres>1</caracteres>
        <maxCaracteres>1</maxCaracteres>
		<permiso>LECTURAESCRITURA</permiso>
		<id>TO-N</id>
	 </campos>
	 <campos nombre="pers_tnombre">
	   <descripcion>Rut</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="pers_tape_paterno">
        <descripcion>Rut</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	 </campos>
 </parametro>
 
 <parametro formulario="fconsultalarga" variable="usuroles" anchoTabla="100%" 
  clave="tiene_rol" nroRegistros="10" maxLista="10" >
	<campos nombre="encu_ncorr">
	   	<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="c_encu_ncorr">
	   	<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="srol_ncorr">
	   	<tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>
	</campos>
	<campos nombre="srol_tdesc">
	   <descripcion>Rol</descripcion>
		<tipo>INPUT</tipo>
	   <permiso>LECTURA</permiso>
	 </campos>
	 <campos nombre="srus_fmodificacion">
	   <descripcion>Fecha Asigando</descripcion>
		<tipo>INPUT</tipo>
	   <permiso>LECTURA</permiso>
	   <alineamiento>CENTER</alineamiento>
	 </campos>
	<campos nombre="tiene_rol">
	   <descripcion>Asignado</descripcion>
		<tipo>BOLEANO</tipo>
	    <objeto>CHECKBOX</objeto>
		<permiso>LECTURAESCRITURA</permiso>
		<alineamiento>CENTER</alineamiento>
	 </campos> 
 </parametro>

 <parametro formulario="botonera">
  <botones nombre="eliminar"> 
      <tipoTexto>ELIMINAR</tipoTexto>
	  <accion>ELIMINAR</accion>
	  <url>nada.asp</url>
	  <formulario>edicion</formulario>
	</botones> 
   <botones nombre="cancelar"> 
      <tipoTexto>SALIR</tipoTexto>
	  <accion>NAVEGAR</accion>
	  <url>../lanzadera/lanzadera.asp</url>
	</botones>
    <botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto>
	  <accion>BUSCAR</accion>
	  <formulario>buscador</formulario>
	  <funcionValidacion>Validar();</funcionValidacion>
	</botones>
	<botones nombre="actualizar"> 
      <tipoTexto>GUARDAR</tipoTexto>
	  <accion>GUARDAR</accion>
	  <url>Proc_m_encu_roles.asp</url>
	  <formulario>edicion</formulario>
	</botones>
</parametro>	

<parametro formulario="f_busqueda" variable="busqueda">
   <campos nombre="encu_ncorr">
      <tipo>SELECT</tipo>
	  <destino>encuestas</destino> 
	  <union>encu_ncorr</union>
	  <salida>encu_tnombre</salida>
      <orden>encu_tnombre asc </orden>	  
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Seleccione una encuesta</mensajeNulo>
	  <mensajeVacio>Seleccione una encuesta</mensajeVacio>
	  <id>TO-N</id>
   </campos>
</parametro>
  
</Parametros>