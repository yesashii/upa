		</campos>

		<campos nombre="hora_ccod">
			<tipo>SELECT</tipo>
			<permiso>LECTURAESCRITURA</permiso>
			<codigo>hora_ccod</codigo>
			<salida>hora_tdesc</salida>
			<anulable>TRUE</anulable>
			<orden>hora_ccod</orden>
