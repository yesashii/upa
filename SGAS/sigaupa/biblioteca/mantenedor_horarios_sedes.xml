			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Descripción Hora</descripcion>
			<alineamiento>LEFT</alineamiento>
		</campos>
		<campos nombre="hora_hinicio">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
			<descripcion>Hora Inicio</descripcion>
			<alineamiento>LEFT</alineamiento>
		</campos>
		<campos nombre="hora_htermino">
