<Parametros>
   <parametro formulario="botonera">
      <botones nombre="siguiente">
         <tipoTexto>SIGUIENTE</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
         <url>pactacion_cargo.asp</url>
         <funcionValidacion>Validar();</funcionValidacion>
      </botones>
      <botones nombre="cancelar">
         <tipoTexto>CANCELAR</tipoTexto>
         <accion>CERRAR</accion>
      </botones>
      <botones nombre="calcular">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Calcular</texto>
         <accion>GUARDAR</accion>
         <formulario>pactacion</formulario>
         <url>proc_calcular_pactacion.asp</url>
         <funcionValidacion>ValidarPactacion();</funcionValidacion>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
      <botones nombre="aceptar_pactacion">
         <tipoTexto>ACEPTAR</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>detalle_pactacion</formulario>
		 <funcionValidacion>ValidarCuotasPago();</funcionValidacion>
         <url>proc_generar_pactacion.asp</url>
         <soloUnClick>TRUE</soloUnClick>
         <mensajeConfirmacion>¿Está seguro que desea generar este cargo?</mensajeConfirmacion>
      </botones>
   </parametro>
   <parametro formulario="cargo" anchoTabla="100%" variable="cargo">
      <campos nombre="pers_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="pers_ncorr_codeudor">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="tdet_ccod">
         <descripcion>Tipo</descripcion>
         <tipo>SELECT</tipo>
         <destino>tipos_detalle</destino>
         <union>tdet_ccod</union>
         <salida>tdet_tdesc</salida>
         <orden>tdet_tdesc asc</orden>
         <filtro>tcom_ccod = 7 and tdet_bvigente = 'S'</filtro>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>TRUE</anulable>
         <script>onChange="tdet_ccod_change(this);"</script>
         <id>TO-N</id>
      </campos>
      <campos nombre="comp_mneto">
         <descripcion>Valor</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>15</caracteres>
         <formato>MONEDA</formato>
         <alineamiento>CENTER</alineamiento>
         <soloLectura>TRUE</soloLectura>
      </campos>
      <campos nombre="comp_mdescuento">
         <descripcion>Descuento</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>15</caracteres>
         <formato>MONEDA</formato>
         <alineamiento>CENTER</alineamiento>
         <onBlur>comp_mdescuento_blur(this);</onBlur>
      </campos>
      <campos nombre="comp_mdocumento">
         <descripcion>Total</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>15</caracteres>
         <formato>MONEDA</formato>
         <alineamiento>CENTER</alineamiento>
         <soloLectura>TRUE</soloLectura>
      </campos>
   </parametro>
