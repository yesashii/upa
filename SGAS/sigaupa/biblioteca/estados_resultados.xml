<Parametros>
   <parametro formulario="botonera">
      <botones nombre="lanzadera">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>Validar();</funcionValidacion>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
	  <botones nombre="imprimir"> 
        <tipoTexto>DEFINIR</tipoTexto>
	    <texto>Imprimir</texto>
		<accion>JAVASCRIPT</accion>
		<formulario>edicion</formulario>		
		<funcion>imprimir();</funcion>
   	 </botones>	  
   </parametro>
   
	<parametro formulario="busqueda" variable="busqueda">
		<campos nombre="sede_ccod">
			 <tipo>SELECT</tipo>
			 <destino>sedes</destino>
			 <union>sede_ccod</union>
			 <salida>sede_tdesc</salida>
			 <orden>sede_tdesc asc</orden>
			 <permiso>LECTURAESCRITURA</permiso>
			 <filtro> sede_ccod in (1,4,8,7) </filtro>
			 <anulable>true</anulable>
			 <mensajeNulo>TODAS LAS SEDES</mensajeNulo>
		</campos>
		<campos nombre="facultad">
			 <tipo>SELECT</tipo>
			 <destino>eru_facultades_upa</destino>
			 <union>cod_facultad</union>
			 <salida>facultad</salida>
			 <orden>cod_facultad asc</orden>
			 <permiso>LECTURAESCRITURA</permiso>
			 <filtro>cod_facultad not in (7,8,9,10,11) </filtro>
			 <anulable>true</anulable>
			 <mensajeNulo>SELECIONE UNA FACULTAD</mensajeNulo>
		</campos>
		
	</parametro>
   
   <parametro formulario="f_presupuesto" clave="envi_ncorr" variable="pre" 
