      <campos nombre="anos_ccod">
         <descripcion>Año</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>8</caracteres>
         <maxCaracteres>4</maxCaracteres>
      </campos>
      <campos nombre="tdet_ccod">
         <descripcion>Curso</descripcion>
         <tipo>SELECT</tipo>
         <destino>tipos_detalle</destino>
         <union>tdet_ccod</union>
         <salida>tdet_tdesc</salida>
         <orden>tdet_tdesc asc</orden>
         <filtro>tcom_ccod=7</filtro>
         <anulable>TRUE</anulable>
         <mensajeNulo>Todos</mensajeNulo>
         <permiso>LECTURAESCRITURA</permiso>
      </campos>
      <campos nombre="formato">
         <columnas>3</columnas>
