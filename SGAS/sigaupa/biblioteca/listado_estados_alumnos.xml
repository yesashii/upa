<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
  <parametro formulario="botonera">
     <botones nombre="lanzadera"> 
        <tipoTexto>SALIR</tipoTexto>
	    <accion>NAVEGAR</accion>
		<url>../lanzadera/lanzadera.asp</url>
	 </botones>
	 <botones nombre="excel">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Excel</texto>
         <accion>GUARDAR</accion>
         <formulario>buscador</formulario>
     </botones>
</parametro>
 
 <parametro formulario="f_busqueda" variable="busqueda">
   <campos nombre="carr_ccod">
      <tipo>SELECT</tipo>
	  <destino>carreras</destino> 
	  <union>carr_ccod</union>
	  <salida>CARR_TDESC</salida>
      <orden>CARR_TDESC asc </orden>	  
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Todas</mensajeNulo>
	  <mensajeVacio>Todas</mensajeVacio>
	  <id>TO-S</id>
   </campos>
   <campos nombre="peri_ccod">
      <tipo>SELECT</tipo>
	  <destino>(select peri_ccod,peri_tdesc from periodos_academicos where anos_ccod=2005) a</destino> 
	  <union>peri_ccod</union>
	  <salida>peri_tdesc</salida>
      <orden>peri_ccod asc </orden>	  
	  <permiso>LECTURAESCRITURA</permiso>
	  <anulable>true</anulable>
	  <mensajeNulo>Todos los contratos</mensajeNulo>
	  <mensajeVacio>Todos los contratos</mensajeVacio>
	  <id>TO-S</id>
   </campos>
   <campos nombre="emat_ccod">
      <tipo>SELECT</tipo>
	  <destino>estados_matriculas</destino> 
	  <union>emat_ccod</union>
	  <salida>emat_tdesc</salida>
      <orden>emat_ccod asc </orden>	  
	  <permiso>LECTURAESCRITURA</permiso>
	  <id>TO-S</id>
   </campos>
  </parametro>
</Parametros>
