<Parametros>
     <parametro formulario="busqueda" columnas="1" camposObligatorios="TRUE" anchoTabla="100%" variable="b">
	  	 <campos nombre="anos_ccod">
     	 <tipo>SELECT</tipo>
	  	 <destino>anos</destino> 
	  	 <union>anos_ccod</union>
	 	 <salida>anos_ccod</salida>
	 	 <permiso>LECTURAESCRITURA</permiso>
	 	 <anulable>true</anulable>
	  	 <alineamiento>Left</alineamiento>
		 <filtro>anos_ccod>2004 and cast(datepart(year,getdate()) as numeric)+1>=anos_ccod</filtro>
	 	 <mensajeNulo>--Año--</mensajeNulo>
	 	 <descripcion>Unidad Fomento por Año</descripcion>
	  	 </campos>
	  </parametro> 
		<parametro formulario="botonera">
       				<botones nombre="buscar">
	      			<tipoTexto>BUSCAR</tipoTexto>
		  			<accion>BUSCAR</accion>
		  			<formulario>buscador</formulario>
				    </botones>
					       <botones nombre="salir">
	      			    	<tipoTexto>SALIR</tipoTexto>
		  				    <accion>NAVEGAR</accion>
						    <url>../lanzadera/lanzadera.asp</url>
						   </botones>
				<botones nombre="AGREGAR">
                <tipoTexto>GUARDAR</tipoTexto>
