<?xml version="1.0" encoding="iso-8859-1"?>
<Parametros>
  <parametro formulario="botonera">
    <botones nombre="buscar"> 
      <tipoTexto>BUSCAR</tipoTexto> 
	  <accion>BUSCAR</accion> 
	  <formulario>buscador</formulario> 
	</botones> 
	<botones nombre="salir"> 
      <tipoTexto>SALIR</tipoTexto> 
	  <accion>JAVASCRIPT</accion> 
	  <funcion>CerrarActualizar();</funcion> 
	</botones>
	<botones nombre="agregar">
         <tipoTexto>GUARDAR</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
         <url>ingreso_calificaciones_proc.asp</url>
    </botones>
	<botones nombre="eliminar">
         <tipoTexto>ELIMINAR</tipoTexto>
         <accion>ELIMINAR</accion>
         <formulario>edicion</formulario>
         <url>toma_carga_otec_eliminar.asp</url>
      </botones>
 </parametro>
 <parametro formulario="busqueda" variable="busqueda">
  		 <listas nombre="lBusqueda">
			<item clave="dgso_ncorr" valor="dgso_tdesc" />
			<item clave="mote_ccod" valor="mote_tdesc" />
			<item clave="seot_ncorr" valor="seot_tdesc" />
		</listas>
		<campos nombre="dgso_ncorr">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
		<campos nombre="mote_ccod">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
		<campos nombre="seot_ncorr">
			<tipo>INPUT</tipo>
			<permiso>LECTURA</permiso>
		</campos>
  </parametro>
  
   <parametro formulario="f_nuevo" variable="n">
		<campos nombre="pers_ncorr">
		  <tipo>INPUT</tipo> 
		  <permiso>OCULTO</permiso> 
		</campos>
		<campos nombre="anos_ccod">
		  <tipo>INPUT</tipo> 
		  <permiso>OCULTO</permiso> 
		</campos>
		 <campos nombre="dgso_ncorr">
			  <tipo>SELECT</tipo>
			  <union>dgso_ncorr</union>
			  <salida>programa</salida>
			  <orden>programa asc </orden>	  
			  <permiso>LECTURAESCRITURA</permiso>
			  <anulable>true</anulable>
			  <mensajeNulo>Seleccione Programa</mensajeNulo>
			  <mensajeVacio>Seleccione Programa</mensajeVacio>
			  <id>TO-N</id>
		 </campos>
  </parametro> 
  <parametro colorSeleccionado="99FFFF" formulario="programas" variable="b">
      <campos nombre="dcur_ncorr">
         <tipo>SELECT</tipo>
         <permiso>lecturaescritura</permiso>
         <union>dcur_ncorr</union>
         <salida>dcur_tdesc</salida>
         <anulable>TRUE</anulable>
         <mensajeNulo>Elija un programa</mensajeNulo>
         <mensajeVacio>No existe matricula de alumno</mensajeVacio>
         <script>onchange='javascript:dibujar(this.form);'</script>
         <id>TO-N</id>
      </campos>
   </parametro>
   <parametro formulario="carga_tomada" variable="p" anchoTabla="100%" nroRegistros="20" maxLista="10" 
   editar="FALSE" paginaEdicion="" eliminar="TRUE" clave="seot_ncorr"
   nuevaVentana="FALSE" mensajeError="No se ha encontrado carga tomada para este alumno">
	  <campos nombre="cod_modulo">
         <descripcion>Código</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="pote_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="modulo">
         <descripcion>Nombre Curso</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="horas">
         <descripcion>Horas Curso</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="horario">
         <descripcion>Horario</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="fecha_carga">
         <descripcion>Fecha de Carga</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
   </parametro>
   <parametro formulario="alumnos" variable="p" anchoTabla="100%" nroRegistros="100" maxLista="10" 
   editar="FALSE" paginaEdicion="" eliminar="FALSE" clave="pote_ncorr"
   nuevaVentana="FALSE" mensajeError="No se ha encontrado alumnos que tomaran este curso">
	  <campos nombre="seot_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
	  <campos nombre="pote_ncorr">
         <tipo>INPUT</tipo>
         <permiso>OCULTO</permiso>
      </campos>
      <campos nombre="rut">
         <descripcion>RUT</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="alumno">
         <descripcion>alumno</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURA</permiso>
      </campos>
	  <campos nombre="caot_nnota_final">
         <descripcion>Calificacion</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <caracteres>5</caracteres>
		 <maxCaracteres>3</maxCaracteres>
		 <alineamiento>CENTER</alineamiento>
		 <id>NT-S</id>
      </campos>
	  <campos nombre="sitf_ccod">
         <tipo>SELECT</tipo>
		 <descripcion>Estado</descripcion>
         <destino>(select sitf_ccod,sitf_tdesc from situaciones_finales where sitf_ccod in ('A','R','C'))a</destino>
		 <permiso>LECTURAESCRITURA</permiso>
         <union>sitf_ccod</union>
         <salida>sitf_tdesc</salida>
         <anulable>TRUE</anulable>
         <mensajeNulo>ESTADO</mensajeNulo>
         <mensajeVacio>ESTADO</mensajeVacio>
         <id>TO-N</id>
      </campos>
	  <campos nombre="caot_nasistencia">
         <descripcion>Asistencia</descripcion>
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
		 <id>NU-N</id>
		 <caracteres>5</caracteres>
		 <maxCaracteres>3</maxCaracteres>
		 <alineamiento>CENTER</alineamiento>
      </campos>
   </parametro>
</Parametros>