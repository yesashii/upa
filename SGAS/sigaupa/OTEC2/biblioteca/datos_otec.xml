<Parametros>
   <parametro formulario="botonera">
      <botones nombre="siguiente">
         <tipoTexto>SIGUIENTE</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
         <url>pactacion_cargo.asp</url>
         <funcionValidacion>Validar(document.edicion);</funcionValidacion>
      </botones>
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>ValidaBusqueda();</funcionValidacion>
      </botones>
      <botones nombre="calcular">
         <tipoTexto>DEFINIR</tipoTexto>
         <texto>Calcular</texto>
         <accion>GUARDAR</accion>
         <formulario>pactacion</formulario>
         <url>proc_calcular_pactacion.asp</url>
         <funcionValidacion>ValidarPactacion();</funcionValidacion>
         <soloUnClick>TRUE</soloUnClick>
      </botones>
      <botones nombre="aceptar_pactacion">
         <tipoTexto>ACEPTAR</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>detalle_pactacion</formulario>
		 <funcionValidacion>ValidarCuotasPago();</funcionValidacion>
         <url>proc_generar_pactacion.asp</url>
         <soloUnClick>TRUE</soloUnClick>
         <mensajeConfirmacion>¿Está seguro que desea generar este cargo?</mensajeConfirmacion>
      </botones> 
      <botones nombre="salir">
         <tipoTexto>SALIR</tipoTexto>
         <accion>NAVEGAR</accion>
         <url>../lanzadera/lanzadera.asp</url>
      </botones>
      <botones nombre="anterior">
         <tipoTexto>ANTERIOR</tipoTexto>
         <accion>NAVEGAR</accion>
      </botones>
	  <botones nombre="cerrar">
         <tipoTexto>CERRAR</tipoTexto>
         <accion>CERRAR</accion>
      </botones>	
   </parametro>

 <parametro formulario="busqueda" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-N</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
         <id>LN-N</id>
      </campos>
      <campos nombre="tipo_persona">
         <descripcion>tipo</descripcion>
         <tipo>SELECT</tipo>
         <destino>(select 1 as codigo_persona,'PERSONA' as tipo_persona union select 2 as codigo_persona,'EMPRESA' as tipo_persona union select 3 as codigo_persona,'OTIC' as tipo_persona) a</destino>
         <union>codigo_persona</union>
         <salida>tipo_persona</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>False</anulable>
         <id>TO-N</id>
      </campos>
   </parametro>

