  <campos nombre="pote_nnota_final">
         <tipo>input</tipo>
         <permiso>lecturaescritura</permiso>
		 <descripcion>Nota Final</descripcion> 
         <caracteres>4</caracteres>
         <maxCaracteres>3</maxCaracteres>
         <alineamiento>center</alineamiento>
         <id>NT-S</id>
  </campos>
  <campos nombre="pote_nasistencia">
         <tipo>input</tipo>
         <permiso>lecturaescritura</permiso>
		 <descripcion>Asistencia(%)</descripcion> 
         <caracteres>3</caracteres>
         <maxCaracteres>3</maxCaracteres>
         <alineamiento>center</alineamiento>
         <id>NU-S</id>
  </campos>
  <campos nombre="pote_nest_final">
         <tipo>SELECT</tipo>
		 <descripcion>Estado Final</descripcion>
         <destino>(SELECT '' AS estado_final, '' AS estado_final_tdesc
		           UNION
		           SELECT '1' AS estado_final, 'REPROBADO' AS estado_final_tdesc
		           UNION
				   SELECT '2' AS estado_final, 'APROBADO' AS estado_final_tdesc)s</destino>
         <union>estado_final</union>
         <salida>estado_final_tdesc</salida>
         <alineamiento>center</alineamiento>
         <permiso>LECTURAESCRITURA</permiso>
         <id>TO-S</id>
  </campos>
