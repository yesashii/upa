   editar="FALSE" 	paginaEdicion="" clave="pote_ncorr" nuevaVentana="FALSE" variable="alumnos">
    <tablas nombre="observaciones_postulacion_otec" accion="M"/>
	<campos nombre="dgso_ncorr">
	   <tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>		
    </campos>
	<campos nombre="pote_ncorr">
	   <tipo>INPUT</tipo>
		<permiso>OCULTO</permiso>		
    </campos>
	<campos nombre="eopo_ccod">
         <tipo>SELECT</tipo>
		 <descripcion>Estado</descripcion>
		 <permiso>LECTURAESCRITURA</permiso>
         <destino>(select eopo_ccod,eopo_tdesc from estado_observaciones_postulacion where eopo_ccod in (1,32,34,35,37,43,44))a</destino>
         <union>eopo_ccod</union>
         <salida>eopo_tdesc</salida>
         <permiso>LECTURAESCRITURA</permiso>
         <anulable>FALSE</anulable>
		 <orden>eopo_tdesc</orden>
         <id>TO-N</id>
      </campos>
	  <campos nombre="obpo_tobservacion">
	    <descripcion>Observación</descripcion>
		<tipo>INPUT</tipo>
		<permiso>LECTURAESCRITURA</permiso>
		<caracteres>50</caracteres>
		<maxCaracteres>150</maxCaracteres>
	 </campos>
   </parametro>
