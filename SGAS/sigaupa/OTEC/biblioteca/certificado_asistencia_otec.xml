<Parametros>
   <parametro formulario="botonera">
	  <botones nombre="anioCalendario">
          <tipoTexto>DEFINIR</tipoTexto> 
          <texto>Año Calendario</texto> 
          <accion>JAVASCRIPT</accion> 
       </botones>
	   
	   <botones nombre="anioParcial">
          <tipoTexto>DEFINIR</tipoTexto> 
          <texto>Año Parcial</texto> 
          <accion>JAVASCRIPT</accion> 
       </botones>
	    <botones nombre="anioComplementario">
          <tipoTexto>DEFINIR</tipoTexto> 
          <texto>Año complementario</texto> 
          <accion>JAVASCRIPT</accion> 
       </botones>
	
      <botones nombre="guardar">
         <tipoTexto>GUARDAR</tipoTexto>
         <accion>GUARDAR</accion>
         <formulario>edicion</formulario>
         <url>proc_certificado_asistencia_otec.asp</url>
      </botones>
      <botones nombre="buscar">
         <tipoTexto>BUSCAR</tipoTexto>
         <accion>BUSCAR</accion>
         <formulario>buscador</formulario>
         <funcionValidacion>ValidaBusqueda();valida_carga();</funcionValidacion>
      </botones>      
   </parametro>

 <parametro formulario="busqueda" variable="busqueda">
      <campos nombre="pers_nrut">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>10</caracteres>
         <maxCaracteres>8</maxCaracteres>
         <id>NU-N</id>
      </campos>
      <campos nombre="pers_xdv">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>1</caracteres>
         <maxCaracteres>1</maxCaracteres>
		 <script>onBlur="this.value=this.value.toUpperCase();"</script>

         <id>LN-N</id>
      </campos>
      <campos nombre="tipo_persona">
         <descripcion>tipo</descripcion>
         <tipo>SELECT</tipo>
         <destino>( select 1 as codigo_persona,'EMPRESA' as tipo_persona union select 2 as codigo_persona, 'OTIC' as tipo_persona ) a</destino>
         <union>codigo_persona</union>
         <salida>tipo_persona</salida>
         <permiso>LECTURAESCRITURA</permiso>
		 <mensajeNulo>Seleccione un tipo</mensajeNulo>
         <anulable>true</anulable>
		 <script>onChange="cambiarTextoD()"</script> 
         <id>TO-N</id>
      </campos>
	   <campos nombre="tipo_doc">
         <tipo>INPUT</tipo>
         <permiso>LECTURAESCRITURA</permiso>
         <caracteres>14</caracteres>
         <maxCaracteres>12</maxCaracteres>
         <id>NU-N</id>
      </campos>
   </parametro>

