  <campos nombre="epot_ccod">
	  <tipo>SELECT</tipo>
	  <destino>(select epot_ccod,epot_tdesc from estados_postulacion_otec where epot_ccod in (2,3))a</destino> 
	  <union>epot_ccod</union>
	  <salida>epot_tdesc</salida>
	  <orden>epot_ccod asc </orden>	  
