	</campos>
  </parametro>
  
   <parametro 
   formulario="f_nuevo" 
   variable="n"
   >
		<campos nombre="pers_ncorr">
		  <tipo>INPUT</tipo> 
		  <permiso>OCULTO</permiso> 
		</campos>
		<campos nombre="anos_ccod">
		  <tipo>INPUT</tipo> 
		  <permiso>OCULTO</permiso> 
		</campos>
		 <campos nombre="dgso_ncorr">
			  <tipo>SELECT</tipo>
			  <union>dgso_ncorr</union>
			  <salida>programa</salida>
			  <orden>programa asc </orden>	  
			  <permiso>LECTURAESCRITURA</permiso>
			  <anulable>true</anulable>
			  <mensajeNulo>Seleccione Programa</mensajeNulo>
			  <mensajeVacio>Seleccione Programa</mensajeVacio>
			  <id>TO-N</id>
		 </campos>
  </parametro> 
  <parametro 
  formulario="programas"
  colorSeleccionado="99FFFF"  
  variable="b"
  >
      <campos nombre="dcur_ncorr">
         <tipo>SELECT</tipo>
         <permiso>lecturaescritura</permiso>
         <union>dcur_ncorr</union>
         <salida>dcur_tdesc</salida>
         <anulable>TRUE</anulable>
         <mensajeNulo>Elija un programa</mensajeNulo>
         <mensajeVacio>No existe matricula de alumno</mensajeVacio>
         <script>onchange='javascript:dibujar(this.form);'</script>
         <id>TO-N</id>
      </campos>
