		<item clave="dgso_ncorr" valor="dgso_tdesc" />
	</listas>
	<campos nombre="anio_admision">
		<tipo>INPUT</tipo>
		<permiso>LECTURA</permiso>
	</campos>
	<campos nombre="dgso_ncorr">
