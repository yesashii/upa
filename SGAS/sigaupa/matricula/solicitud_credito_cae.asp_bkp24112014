<script language="JavaScript">
function ValidaFormBusqueda()
{
	var formulario = document.buscador;
	var	rut = formulario.elements["busqueda[0][pers_nrut]"].value + '-' + formulario.elements["busqueda[0][pers_xdv]"].value;
	
	if (!valida_rut(rut)) {
		alert('Ingrese un RUT válido.');
		formulario.elements["busqueda[0][pers_xdv]"].select();
		return false;
	}
	
	return true;
	
}
function InicioPagina(formulario){

}

function imprimir() {
  var rut;
  var direccion;
  post_ncorr=<%=v_post_ncorr_carrera%>
  ofer_ncorr=<%=v_oferta%>
  rut=<%=q_pers_nrut%>
  direccion="imprimir_solicitud_cae.asp?pers_nrut="+rut+"&post_ncorr="+post_ncorr+"&ofer_ncorr="+ofer_ncorr;
  window.open(direccion ,"ventana1","width=680,height=680,scrollbars=yes, left=350, top=150, resizable=yes");
}

function ValidaRadios(formulario){

	if((!formulario.elements["cae[0][socc_bsolicita]"][0].checked)&&(!formulario.elements["cae[0][socc_bsolicita]"][1].checked)){
		alert("Debe seleccionar la opcion de solicitar o no solictar credito Cae");
		return false;
	}
	
	if((!formulario.elements["cae[0][socc_brenovante]"][0].checked)&&(!formulario.elements["cae[0][socc_brenovante]"][1].checked)){
		alert("Debe seleccionar el tipo de alumno Cae");
		return false;
	}
	
	if((!formulario.elements["cae[0][socc_bcurso_superior]"][0].checked)&&(!formulario.elements["cae[0][socc_bcurso_superior]"][1].checked)){
		alert("Debe seleccionar si es curso superior o no");
		return false;
	}
	
	if (formulario.elements["cae[0][socc_bsolicita]"][0].checked){
		if(formulario.elements["cae[0][socc_mmonto_solicitado]"].value==0){
			alert("Debe ingresar un valor a solicitar");
			return false;
		}	
	
		if((parseInt(formulario.elements["arancel_referencia"].value))<(parseInt(formulario.elements["cae[0][socc_mmonto_solicitado]"].value))){
			alert("No puede ingresar un monto mayor al arancel de referencia: "+formulario.elements["arancel_referencia"].value);
			return false;
		}
		
		if((parseInt(formulario.elements["cae[0][socc_mmonto_solicitado]"].value)>0)&&(parseInt(formulario.elements["cae[0][socc_mmonto_solicitado]"].value)<200000)){
			alert("El monto solicitado no puede ser inferior a $200.000");
			return false;
		}
	}
		
	formulario.method="POST";
	formulario.action="proc_solicitud_credito_cae.asp";
	formulario.submit();
	
}

function CopiaMonto(valor){
	var formulario = document.edicion;
	if (valor==1){
		formulario.elements["cae[0][socc_mmonto_solicitado]"].value=formulario.elements["arancel_referencia"].value;
		formulario.elements["cae[0][socc_mmonto_solicitado]"].readOnly=true;
	}else{
		formulario.elements["cae[0][socc_mmonto_solicitado]"].readOnly=false;
		formulario.elements["cae[0][socc_mmonto_solicitado]"].focus();
		formulario.elements["cae[0][socc_mmonto_solicitado]"].value=0;				
	}
}

function Solicita(valor){
	var formulario = document.edicion;
	if (valor==1){
		formulario.elements["cae[0][socc_mmonto_solicitado]"].value=0;
		formulario.elements["cae[0][socc_mmonto_solicitado]"].readOnly=false;
		formulario.elements["cae[0][socc_bmonto_solicitado]"][0].disabled=false;
		formulario.elements["cae[0][socc_bmonto_solicitado]"][1].checked=false;		
	}else{
		formulario.elements["cae[0][socc_mmonto_solicitado]"].value=0;
		formulario.elements["cae[0][socc_mmonto_solicitado]"].readOnly=true;
		formulario.elements["cae[0][socc_bmonto_solicitado]"][0].disabled=true;
		formulario.elements["cae[0][socc_bmonto_solicitado]"][1].checked=true;		
	}
}


function EsNumerico(obj) {
   value = obj.value;
   cont=0;
   for (n = 0; n < value.length; n++){

	  if ( ! isDigit(value.charAt(n))) {
         alert("Debe ingresar un valor numerico");
		 obj.value="";
         return false;
      }
	}
}

function isDigit(ch) {
   if (ch >= '0' && ch <= '9')
      return true;
   return false;
}

