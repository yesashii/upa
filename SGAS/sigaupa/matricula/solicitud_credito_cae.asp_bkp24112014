<%
q_pers_nrut = Request.QueryString("busqueda[0][pers_nrut]")
q_pers_xdv = Request.QueryString("busqueda[0][pers_xdv]")
v_post_ncorr = Request.QueryString("busqueda[0][post_ncorr]")

'---------------------------------------------------------------------------------------------------
set pagina = new CPagina
pagina.Titulo = "CREDITO CON AVAL DEL ESTADO LEY N° 20.027"

'---------------------------------------------------------------------------------------------------
set conexion = new CConexion
conexion.Inicializar "upacifico"

set negocio = new CNegocio
negocio.Inicializa conexion

v_peri_ccod = negocio.ObtenerPeriodoAcademico("POSTULACION")


if v_peri_ccod<>"234" then
	v_peri_ccod="234"
end if

v_anio= conexion.consultaUno("select anos_ccod from periodos_academicos where peri_ccod="&v_peri_ccod)

'#######################################################################
'#############	consultas para obtener los cupos cae	################
set f_cupos_cae = new CFormulario
f_cupos_cae.Carga_Parametros "tabla_vacia.xml", "tabla_vacia"
f_cupos_cae.Inicializar conexion

sql_cupos_cae="select ccae_ncupos_nuevos as cupos_nuevos,ccae_ncupos_superiores as cupos_superiores, isnull(ccae_ncupos_nuevos,0) + isnull(ccae_ncupos_superiores,0) as total_cupos from ufe_cupos_caes where anos_ccod="&v_anio

'response.Write(sql_cupos_cae)

f_cupos_cae.Consultar sql_cupos_cae
f_cupos_cae.Siguiente

'----------------------------------------------------------------------------------
set f_cupos_reales = new CFormulario
f_cupos_reales.Carga_Parametros "tabla_vacia.xml", "tabla_vacia"
f_cupos_reales.Inicializar conexion

sql_cupos_reales= " select isnull(sum(nuevo),0) as nuevos, isnull(sum(superior),0) as superiores,  isnull(sum(superior)+sum(nuevo),0) as total " & vbcrlf & _
				"	from ( " & vbcrlf & _
				"	select case when isnull(socc_bcurso_superior,0) =1 then 1 end as nuevo,case when isnull(socc_bcurso_superior,0) =2 then 1 end as superior  " & vbcrlf & _
				"	 from solicitud_credito_cae a  " & vbcrlf & _
				"	join postulantes b " & vbcrlf & _
				"		on a.post_ncorr=b.POST_NCORR  " & vbcrlf & _
				"	join alumnos c " & vbcrlf & _
				"		on b.post_ncorr=c.POST_NCORR " & vbcrlf & _
				"	join ofertas_academicas d " & vbcrlf & _
				"		on c.ofer_ncorr=d.OFER_NCORR " & vbcrlf & _
				"	join periodos_academicos e " & vbcrlf & _
				"		on d.peri_ccod=e.peri_ccod " & vbcrlf & _
				"	where e.anos_ccod="&v_anio&" " & vbcrlf & _
				"		and convert(datetime,a.audi_fmodificacion,103) >= convert(datetime,'13/01/2014',103)  " & vbcrlf & _
				"	) as tabla " 

'response.Write(sql_cupos_reales)

f_cupos_reales.Consultar sql_cupos_reales
f_cupos_reales.Siguiente

'#######################################################################

'---------------------------------------------------------------------------------------------------
set f_botonera = new CFormulario
f_botonera.Carga_Parametros "solicitud_credito_cae.xml", "botonera"

if not EsVacio(v_post_ncorr)  then
	v_datos=split(v_post_ncorr,"&")
	v_post_ncorr_carrera=v_datos(0)
	v_oferta=v_datos(1)
	
	sql_update="Update postulantes set ofer_ncorr="&v_oferta&" where post_ncorr="&v_post_ncorr_carrera&" "
	'response.Write(sql_update)
	conexion.ejecutaS(sql_update)
	v_existe=conexion.consultaUno("select count(*) from solicitud_credito_cae where post_ncorr="&v_post_ncorr_carrera&" and ofer_ncorr="&v_oferta&" ")
	
end if

'---------------------------------------------------------------------------------------------------
set f_busqueda = new CFormulario
f_busqueda.Carga_Parametros "solicitud_credito_cae.xml", "busqueda"
f_busqueda.Inicializar conexion

f_busqueda.Consultar "select ''"
f_busqueda.Siguiente
f_busqueda.AgregaCampoCons "pers_nrut", q_pers_nrut
f_busqueda.AgregaCampoCons "pers_xdv", q_pers_xdv
f_busqueda.AgregaCampoCons "post_ncorr", v_post_ncorr

consulta_select = "(select cast(b.post_ncorr as varchar)+'&'+cast(bb.ofer_ncorr as varchar) as post_ncorr, e.carr_tdesc as carrera -- +'-'+ d.espe_tdesc as carrera " & vbcrlf & _ 
				 " from personas_postulante a, postulantes b, detalle_postulantes bb, ofertas_academicas c, " & vbcrlf & _  
                 " especialidades d, carreras e " & vbcrlf & _ 
				 " where a.pers_ncorr = b.pers_ncorr " & vbcrlf & _  
				 "  and bb.ofer_ncorr = c.ofer_ncorr " & vbcrlf & _ 
				 "  --and b.ofer_ncorr =c.ofer_ncorr " & vbcrlf & _  
				 "  and b.post_ncorr = bb.post_ncorr " & vbcrlf & _  
				 "  and c.espe_ccod = d.espe_ccod " & vbcrlf & _  
				 "  and d.carr_ccod = e.carr_ccod " & vbcrlf & _  
				 "  and b.tpos_ccod in (1,2) " & vbcrlf & _  
				 "  and b.epos_ccod = 2  " & vbcrlf & _ 
				 "  and b.peri_ccod =  " & v_peri_ccod & " " & vbcrlf & _ 
				 "  and cast(a.pers_nrut as varchar)=  '" & q_pers_nrut & "') a "

'response.Write("<pre>"&consulta_select&"</pre>")
				 
f_busqueda.AgregaCampoParam "post_ncorr", "destino", consulta_select

v_cantidad_carrera=conexion.consultaUno("Select count(*) from "&consulta_select&" ")

v_pers_ncorr = conexion.consultauno("select pers_ncorr from personas_postulante where cast(pers_nrut as varchar)  = '"&q_pers_nrut&"'")	

'--------------------------------------------------------------------------------------------------
set fc_datos = new CFormulario
fc_datos.Carga_Parametros "consulta.xml", "consulta"
fc_datos.Inicializar conexion

		   
consulta = "select cast(a.pers_nrut as varchar) + ' - ' + a.pers_xdv as rut," & vbCrLf &_
			"         a.pers_tnombre + ' ' + a.pers_tape_paterno + ' ' + a.pers_tape_materno as nombre_completo " & vbCrLf &_
			"from personas_postulante a " & vbCrLf &_
			"where cast(a.pers_nrut as varchar) = '" & q_pers_nrut & "'"
'response.Write("<pre>"&consulta&"</pre>")
'response.End()		   
fc_datos.Consultar consulta
fc_datos.Siguiente
'response.Write(fc_datos.nrofilas)


if not esVacio(v_oferta) and v_oferta <> "" then

	set f_solicitud = new CFormulario
	f_solicitud.Carga_Parametros "solicitud_credito_cae.xml", "solicitud_credito_cae"
	f_solicitud.Inicializar conexion
	
	if v_existe>=1 then
		sql_datos_solicitud=" select * from solicitud_credito_cae "&_
							" where post_ncorr="&v_post_ncorr_carrera&" "&_ 
							" and ofer_ncorr="&v_oferta&" "
	else
		sql_datos_solicitud="select "&v_oferta&" as ofer_ncorr,"&v_post_ncorr_carrera&" as post_ncorr,"&v_pers_ncorr&" as pers_ncorr, "&_
							" 0 as socc_mmonto_solicitado "						
	end if

'response.Write(sql_datos_solicitud)
							
	f_solicitud.Consultar sql_datos_solicitud
	f_solicitud.Siguiente						

'** AGREGADO POR JUAN PABLO DURAN ******
	cod_carrera = conexion.consultaUno("select carr_ccod from ofertas_academicas b,especialidades c where cast(peri_ccod as varchar)='"&v_peri_ccod&"' and b.espe_ccod = c.espe_ccod and b.ofer_ncorr="&v_oferta&" ")
	carrera = conexion.consultaUno("select upper(carr_tdesc) from carreras where carr_ccod ='"&cod_carrera&"'")	
	ano_ingreso = conexion.consultaUno("select isnull(protic.ANO_INGRESO_CARRERA("&v_pers_ncorr&",'"&cod_carrera&"'),year(getdate()))")
	sede_ccod = conexion.consultaUno("select sede_ccod from ofertas_academicas where ofer_ncorr="&v_oferta&" ")
	sede = conexion.consultaUno("select Upper(sede_tdesc) from sedes where sede_ccod ='"&sede_ccod&"'")	
	jornada = conexion.consultaUno("select upper(jorn_tdesc) from ofertas_academicas a, jornadas b where a.ofer_ncorr="&v_oferta&" and a.jorn_ccod=b.jorn_ccod ")

	
	v_arancel_real=conexion.consultaUno("select max(aran_mcolegiatura) from aranceles where ofer_ncorr="&v_oferta&" ")
	v_arancel_real=conexion.consultaUno("select  top 1 aran_mcolegiatura from aranceles where ofer_ncorr="&v_oferta&" order by audi_fmodificacion desc")
	
	sql_arancel_referencia= "select min(isnull(b.arancel_ext,0)) as arancel_referencia "&_
							"	from aranceles a, especialidades c, ufe_aranceles_ext b,ofertas_academicas d "&_
							"	where a.sede_ccod=b.sede_ccod "&_
							"	and d.jorn_ccod=b.jorn_ccod "&_
							"	and a.espe_ccod=c.espe_ccod "&_
							"	and c.carr_ccod=b.carr_ccod "&_
							"	and c.espe_ccod=d.espe_ccod "&_
							"	and b.anos_ccod= year(getdate()) "&_
							"	and a.ofer_ncorr="&v_oferta&" "
	
	
	'solución básica a problema de sede Baquedano-Las Condes
	c_ultima_sede = "select top 1 b.sede_ccod from alumnos a, ofertas_academicas b where a.ofer_ncorr=b.ofer_ncorr and cast(a.pers_ncorr as varchar)='"&v_pers_ncorr&"' order by b.peri_ccod desc"
	ultima_sede = conexion.consultaUno(c_ultima_sede)
	if ultima_sede = "8" and v_peri_ccod="234" then
	sql_arancel_referencia= "select min(isnull(b.arancel_ext,0)) as arancel_referencia "&_
							"	from aranceles a, especialidades c, ufe_aranceles_ext b,ofertas_academicas d "&_
							"	where b.sede_ccod = 8 "&_
							"	and d.jorn_ccod=b.jorn_ccod "&_
							"	and a.espe_ccod=c.espe_ccod "&_
							"	and c.carr_ccod=b.carr_ccod "&_
							"	and c.espe_ccod=d.espe_ccod "&_
							"	and b.anos_ccod= year(getdate()) "&_
							"	and a.ofer_ncorr="&v_oferta&" "
	end if
	'Fin del parche - Marcelo Sandoval 26-12-2013
	
	'response.Write(sql_arancel_referencia)
	v_arancel_referencia=conexion.consultaUno(sql_arancel_referencia)
							
end if

if  q_pers_nrut<>"" then
query_estado_respaldado="select case  when count(*)>0 then 'N' else 'S' end estado_respaldo  from ufe_no_respaldados_cae where pers_ncorr=protic.obtener_pers_ncorr(" & q_pers_nrut & ") and anos_ccod=2014"
respaldado=conexion.consultaUno(query_estado_respaldado)
else
respaldado="S"
end  if

if  q_pers_nrut<>"" and v_post_ncorr_carrera<>"" and v_oferta<>"" then 
query_tiene_beca_copago="select case count(*) when 0 then 'N' else 'S' end as tiene  from sdescuentos where post_ncorr="&v_post_ncorr_carrera&" and ofer_ncorr="&v_oferta&" and STDE_CCOD=2210"
'response.Write(query_tiene_beca_copago)
tiene_beca_copago=conexion.consultaUno(query_tiene_beca_copago)


query_procentaje="select SDES_NPORC_COLEGIATURA from sdescuentos where post_ncorr="&v_post_ncorr_carrera&" and ofer_ncorr="&v_oferta&" and STDE_CCOD=2210"
procentaje=conexion.consultaUno(query_procentaje)
else
tiene_beca_copago="N"
end  if

'response.Write(tiene_beca_copago)
