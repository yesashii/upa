	  <script type="text/javascript">
          google.load('visualization', '1.1', {packages: ['controls']});
      </script>
      <script type="text/javascript">
      function drawVisualization() {
        // Prepare the data
        var data = google.visualization.arrayToDataTable([
          ['Sede', 'Sexo', 'Periodo', 'Total']
		       <%Dim obj0
                  if chequeo_2005 <> "" then
				  For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2005', <%=Html.Encode(obj0.a2005) %>]
				<%next
				  end if%>
			   <%if chequeo_2006 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2006', <%=Html.Encode(obj0.a2006) %>]
				<%next
				  end if%>
			   <%if chequeo_2007 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2007', <%=Html.Encode(obj0.a2007) %>]
			   <%next
				 end if%>	
			   <%if chequeo_2008 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2008', <%=Html.Encode(obj0.a2008) %>]
			   <%next
				 end if%>
			   <%if chequeo_2009 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2009', <%=Html.Encode(obj0.a2009) %>]
			   <%next
				 end if%>
			   <%if chequeo_2010 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2010', <%=Html.Encode(obj0.a2010) %>]
			   <%next
				 end if%>
			   <%if chequeo_2011 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2011', <%=Html.Encode(obj0.a2011) %>]
			   <%next
				 end if%>
			   <%if chequeo_2012 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2012', <%=Html.Encode(obj0.a2012) %>]
			   <%next
				 end if%>
			   <%if chequeo_2013 <> "" then
			     For each obj0 in Model.Items%>
                  ,['<%=Html.Encode(obj0.cabecera)%>','<%=Html.Encode(obj0.sexo)%>', '2013', <%=Html.Encode(obj0.a2013) %>]
			   <%next
				 end if%>
        ]);
      
        // Define category pickers for 'Country', 'Region/State' and 'City'
        var sedePicker = new google.visualization.ControlWrapper({
          'controlType': 'CategoryFilter',
          'containerId': 'control1',
          'options': {
            'filterColumnLabel': 'Sede',
            'ui': {
              'labelStacking': 'horizontal',
              'allowTyping': false,
              'allowMultiple': false
            }
          }
        });
      
        var sexoPicker = new google.visualization.ControlWrapper({
          'controlType': 'CategoryFilter',
          'containerId': 'control2',
          'options': {
            'filterColumnLabel': 'Sexo',
            'ui': {
              'labelStacking': 'horizontal',
              'allowTyping': false,
              'allowMultiple': false
            }
          }
        });
      
        var periodoPicker = new google.visualization.ControlWrapper({
          'controlType': 'CategoryFilter',
          'containerId': 'control3',
          'options': {
            'filterColumnLabel': 'Periodo',
            'ui': {
              'labelStacking': 'horizontal',
              'allowTyping': false,
              'allowMultiple': true
            }
          }
        });
      
        // Define a bar chart to show 'Population' data
        var pieChart = new google.visualization.ChartWrapper({
          'chartType': 'ColumnChart',
          'containerId': 'chart1',
          'options': {
            'width': 300,
            'height': 300,
			//'isStacked' : true,
			'legend':'yes',
            'chartArea': {top: 0, right: 0, bottom: 0}
          },
          // Configure the barchart to use columns 2 (City) and 3 (Population)
          'view': {'columns': [0, 3]}
        });
      
	    // Define a table
        var table = new google.visualization.ChartWrapper({
          'chartType': 'Table',
          'containerId': 'chart2',
          'options': {
            'width': '500px'
          }
        });
	     
        // Create the dashboard.
        new google.visualization.Dashboard(document.getElementById('dashboard')).
          // Configure the controls so that:
          // - the 'Sede' selection drives the 'Sexos' one,
          // - the 'Sexo' selection drives the 'Periodos' one,
          // - and finally the 'Periodo' output drives the chart
          bind(sedePicker, sexoPicker).
          bind(sexoPicker, periodoPicker).
          bind(periodoPicker, [pieChart,table]).
          // Draw the dashboard
          draw(data);
      }
      

      google.setOnLoadCallback(drawVisualization);
    </script>
